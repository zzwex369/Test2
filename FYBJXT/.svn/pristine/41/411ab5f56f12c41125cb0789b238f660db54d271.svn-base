			public var systemUser:SystemUser = null;
			
			private function moduleCreationCompleteHandler(event:FlexEvent):void
			{
				this.systemUser = this.parentApplication.systemUser;
				
				this.initilizeComboBox();

			}
			
			private function initilizeComboBox():void
			{
				
			}
			
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				
			}
			
			
			private function query(event:MouseEvent):void
			{
				
				if(this.d501_12.selectedDate != null && this.d501_12_end.selectedDate != null) {
					var r:int = ObjectUtil.compare(this.d501_12.selectedDate, this.d501_12_end.selectedDate);
					if(r == 1) {
						Alert.show("查询起始时间不能大于终止时间!", "系统提示");
						return;
					}
				}
				
				var queryBirthRequest:QueryBirthRequest = new QueryBirthRequest();
				
				var d301:D301 = new D301();
				
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_42 = '0';
				queryBirthRequest.parameterPageindex = 1;
				queryBirthRequest.parameterPagesize = 8;

				this.certificateApplyService.queryB(queryBirthRequest);
				
				queryBirthRequest = new QueryBirthRequest();
				
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_42 = '1';
				queryBirthRequest.parameterPageindex = 1;
				queryBirthRequest.parameterPagesize = 8;
				//HF
				this.certificateApplyService.queryC(queryBirthRequest);
				
				queryBirthRequest = new QueryBirthRequest();
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_42 = '2';
				queryBirthRequest.parameterPageindex = 1;
				queryBirthRequest.parameterPagesize = 8;
				
				//BF
				this.certificateApplyService.queryD(queryBirthRequest);
				
				var d401Request:D401Request = new D401Request();
				var d401:D401 = new D401();
				
				d401.d40113 = this.area.getAreaCode();
				d401.d40108 = this.d501_12.selectedDate;
				d401.d40108_end = this.d501_12_end.selectedDate;
				
				d401.d40105 = this.d501_06.text;
				d401Request.d401 = d401;
				d401Request.parameterPageindex = 1;
				d401Request.parameterPagesize = 8;
				//ZF
				this.certificateApplyService.queryE(d401Request);
			}
			
			private function queryBHandler(event:ResultEvent):void
			{
				var queryBirthResponse:Object = event.result
				if(queryBirthResponse.errorMessage == null) {
					var vd301s:ArrayCollection = queryBirthResponse.listVD301;
					var rowCount:Number = queryBirthResponse.rowCount;
					this.pagerBarSign.pageSize = 8;
					this.pagerBarSign.dataGrid = this.SignDataGrid;
					this.pagerBarSign.resultData = vd301s;
					this.pagerBarSign.rowCount = rowCount;
					this.pagerBarSign.pagerFunction = pagerFunctionSign;
					this.pagerBarSign.dataBind();
					if(vd301s.length <= 0) {
						/*this.signVG.visible = false;
						this.signVG.width = 0;
						this.signVG.height = 0;
						*/
						this.pagerBarSign.enabled = false;
						if(this.top.containsElement(this.signVG)) {
							this.top.removeElement(this.signVG);
						}
						
					} else {
						/*
						this.signVG.visible = true;
						this.signVG.percentWidth = 100;
						this.signVG.height = 250;*/
						
						this.top.addElementAt(this.signVG, 0);
						this.pagerBarSign.enabled = true;
					}
				} else {
					Alert.show(queryBirthResponse.errorMessage, "系统异常");
					return;
				}
			}
				
			public function pagerFunctionSign(currentPageIndex:int, pageSize:int):void {
				var queryBirthRequest:QueryBirthRequest = new QueryBirthRequest();
				var d301:D301 = new D301();
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_42 = '0';
				queryBirthRequest.parameterPageindex = currentPageIndex;
				queryBirthRequest.parameterPagesize = pageSize;
				//QF
				this.certificateApplyService.queryB(queryBirthRequest);
			}
			
			private function queryCHandler(event:ResultEvent):void
			{
				var queryBirthResponse:Object = event.result;
				if(queryBirthResponse.errorMessage == null) {
					var vd301s:ArrayCollection = queryBirthResponse.listVD301;
					var rowCount:Number = queryBirthResponse.rowCount;
					this.pagerBarChange.pageSize = 8;
					this.pagerBarChange.dataGrid = this.ChangeDataGrid;
					this.pagerBarChange.resultData = vd301s;
					this.pagerBarChange.rowCount = rowCount;
					this.pagerBarChange.pagerFunction = pagerFunctionChange;
					this.pagerBarChange.dataBind();
					if(vd301s.length <= 0) {
						/*
						this.changeVG.visible = false;
						this.changeVG.width = 0;
						this.changeVG.height = 0;
						*/
						this.pagerBarChange.enabled = false;
						if(this.top.containsElement(this.changeVG)) {
							this.top.removeElement(this.changeVG);
						}
						
					} else  {
						/*
						this.changeVG.visible = true;
						this.changeVG.percentWidth = 100;
						this.changeVG.height = 250;*/
						this.top.addElementAt(this.changeVG, 1);
						this.pagerBarChange.enabled = true;
					}
				} else {
					Alert.show(queryBirthResponse.errorMessage, "系统异常");
					return;
				}
			}
			
			public function pagerFunctionChange(currentPageIndex:int, pageSize:int):void
			{
				var queryBirthRequest:QueryBirthRequest = new QueryBirthRequest();
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_42 = '1';
				queryBirthRequest.parameterPageindex = currentPageIndex;
				queryBirthRequest.parameterPagesize = pageSize;
				//HF
				this.certificateApplyService.queryC(queryBirthRequest);
			}
			
			private function queryDHandler(event:ResultEvent):void
			{
				var queryBirthResponse:Object = event.result
				if(queryBirthResponse.errorMessage == null) {
					var vd301s:ArrayCollection = queryBirthResponse.listVD301;
					var rowCount:Number = queryBirthResponse.rowCount;
					this.pagerBarReplace.pageSize = 8;
					this.pagerBarReplace.dataGrid = this.ReplaceDataGrid;
					this.pagerBarReplace.resultData = vd301s;
					this.pagerBarReplace.rowCount = rowCount;
					this.pagerBarReplace.pagerFunction = pagerFunctionReplace;
					this.pagerBarReplace.dataBind();
					if(vd301s.length <= 0) {
						/*
						this.replaceVG.visible = false;
						this.replaceVG.width = 0;
						this.replaceVG.height = 0;*/
						this.pagerBarReplace.enabled = false;
						if(this.top.containsElement(this.replaceVG)) {
							this.top.removeElement(this.replaceVG);
						}
						
					} else { 
						this.top.addElementAt(this.replaceVG, 2);
						/*
						this.replaceVG.visible = true;
						this.replaceVG.percentWidth = 100;
						this.replaceVG.height = 250;*/
						this.pagerBarReplace.enabled = true;
					}
				} else {
					Alert.show(queryBirthResponse.errorMessage, "系统异常");
					return;
				}
			}
			
			public function pagerFunctionReplace(currentPageIndex:int, pageSize:int):void {
				var queryBirthRequest:QueryBirthRequest = new QueryBirthRequest();
				queryBirthRequest.d301_28Start = this.d501_12.selectedDate;
				queryBirthRequest.d301_28End = this.d501_12_end.selectedDate;
				queryBirthRequest.d301_10 = this.d501_06.text;
				queryBirthRequest.d301_31 = this.area.getAreaCode();
				queryBirthRequest.d301_42 = '2';
				queryBirthRequest.parameterPageindex = currentPageIndex;
				queryBirthRequest.parameterPagesize = pageSize;
				//BF
				this.certificateApplyService.queryD(queryBirthRequest);
			}
			
			private function queryEHandler(event:ResultEvent):void
			{
				var d401Response:Object = event.result;
				if(d401Response.errorMessage == null) {
					var vd401s:ArrayCollection = d401Response.vd401s;
					var rowCount:Number = d401Response.rowCount;
					this.pagerBarAbandon.pageSize = 8;
					this.pagerBarAbandon.dataGrid = this.AbandonDataGrid;
					this.pagerBarAbandon.resultData = vd401s;
					this.pagerBarAbandon.rowCount = rowCount;
					this.pagerBarAbandon.pagerFunction = pagerFunctionAbandon;
					this.pagerBarAbandon.dataBind();
					if(vd401s.length <= 0) {
						this.pagerBarAbandon.enabled = false;
						if(this.top.containsElement(this.abandonVG)) {
							this.top.removeElement(this.abandonVG);
						}
						/*
						this.abandonVG.visible = false;
						this.abandonVG.width = 0;
						this.abandonVG.height = 0;
						*/
						
					} else {
						/*
						this.abandonVG.visible = true;
						this.abandonVG.percentWidth = 100;
						this.abandonVG.height = 250;
						*/
						this.top.addElementAt(this.abandonVG, 3);
						this.pagerBarAbandon.enabled = true;
					}
				} else {
					Alert.show(d401Response.errorMessage, "系统异常");
					return;
				}
			}
			
			public function pagerFunctionAbandon(currentPageIndex:int, pageSize:int):void
			{
				var d401Request:D401Request = new D401Request();
				var d401:D401 = new D401();
				d401.d40108 = this.d501_12.selectedDate;
				d401.d40108_end = this.d501_12_end.selectedDate;
				d401.d40105 = this.d501_06.text;
				d401.d40113 = this.area.getAreaCode();
				d401Request.d401 = d401;
				d401Request.parameterPageindex = currentPageIndex;
				d401Request.parameterPagesize = pageSize;
				//ZF
				this.certificateApplyService.queryE(d401Request);
			}
			
			private function formatDateToStringD50112(vd501:Object, column:GridColumn):String
			{
				var dateTimeFormatter:DateTimeFormatter = new DateTimeFormatter();
				dateTimeFormatter.dateTimePattern = "yyyy-MM-dd";
				var date_D50112:Date = vd501.d50112;
				var date_D50112_Text:String = dateTimeFormatter.format(date_D50112);
				return date_D50112_Text;
			}
			
			private function faultHandler(event:FaultEvent):void
			{
				var sessionException:SessionException = event.fault.rootCause as SessionException;
				var errorCode:String = "";
				try {
					errorCode = sessionException.errorCode;
					if(errorCode == SystemConstant.LOGIN_USER_INFO_IS_NULL) {
						Alert.show(sessionException.errorMessage, "系统提示");
						return;
					}
				} catch(error:Error) {
					Alert.show("系统异常", "系统提示");
				}
			}
			private function formatDateToStringD40104AndD40108(vd401:Object, column:GridColumn):String
			{
				var dateTimeFormatter:DateTimeFormatter = new DateTimeFormatter();
				dateTimeFormatter.dateTimePattern = "yyyy-MM-dd";
				var columnDataField:String = column.dataField;
				if("d40104" == columnDataField) {
					var date_D40104:Date = vd401.d40104;
					var date_D40104_Text:String = dateTimeFormatter.format(date_D40104);
					return date_D40104_Text;
				} else if("d40108" == columnDataField) {
					var date_D40108:Date = vd401.d40108;
					var date_D40108_Text:String = dateTimeFormatter.format(date_D40108);
					return date_D40108_Text;
				}
				return null;
			}
		]]>
	</fx:Script>
