				var columnDataField:String = column.dataField;
				if("d40104" == columnDataField) {
					var date_D40104:Date = vd401.d40104;
					var date_D40104_Text:String = dateTimeFormatter.format(date_D40104);
					return date_D40104_Text;
				} else if("d40108" == columnDataField) {
					var date_D40108:Date = vd401.d40108;
					var date_D40108_Text:String = dateTimeFormatter.format(date_D40108);
					return date_D40108_Text;
				}
				return null;
			}
			
			private function queryEHandler(event:ResultEvent):void
			{
				var d401Response:Object = event.result;
				if(d401Response.errorMessage == null) {
					var vd401s:ArrayCollection = d401Response.vd401s;
					var rowCount:Number = d401Response.rowCount;
					this.pagerBarAbandon.pageSize = 8;
					this.pagerBarAbandon.dataGrid = this.AbandonDataGrid;
					this.pagerBarAbandon.resultData = vd401s;
					this.pagerBarAbandon.rowCount = rowCount;
					this.pagerBarAbandon.pagerFunction = pagerFunctionAbandon;
					this.pagerBarAbandon.dataBind();
					if(vd401s.length <= 0) {
						this.abandonVG.visible = false;
						this.abandonVG.width = 0;
						this.abandonVG.height = 0;
					} else {/*if(this.d501_06.text != "") {
						this.AbandonDataGrid.visible = true;
						this.abandonL.visible = true
						for(var i:int = 0; i < vd401s.length; i++)
						{
						var vd401:VD401 = vd401s.getItemAt(i) as VD401;
						if(vd401.d40105 == this.d501_06.text) {
						this.AbandonDataGrid.dataProvider = new ArrayCollection([vd401s.getItemAt(i)]);
						return;
						} else {
						this.AbandonDataGrid.dataProvider = new ArrayCollection();
						return;
						}
						}
						}*/
						this.abandonVG.visible = true;
						this.abandonVG.percentWidth = 100;
						this.abandonVG.height = 250;
						//this.pagerBarAbandon.currentPageIndex = 1;
						//this.AbandonDataGrid.dataProvider = vd401s;
					}
				} else {
					Alert.show(d401Response.errorMessage, "系统异常");
					return;
				}
			}
			public function pagerFunctionAbandon(currentPageIndex:int, pageSize:int):void
			{
				var d401Request:D401Request = new D401Request();
				var d401:D401 = new D401();
				/*
				d401.d40108 = this.d501_12.selectedDate;
				d401.d40108_end = this.d501_12_end.selectedDate;
				*/
				d401.d40110 = this.vd501.d50102;
				d401.d40114 = this.vd501.d50101;
				d401.d40105 = this.d501_06.text;
				d401Request.d401 = d401;
				d401Request.parameterPageindex = currentPageIndex;
				d401Request.parameterPagesize = pageSize;
				//ZF
				this.certificateApplyService.queryE(d401Request);
			}
		]]>
