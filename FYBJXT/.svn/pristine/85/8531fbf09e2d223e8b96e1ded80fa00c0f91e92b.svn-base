			private function getDropDownListSelectedIndex(arrayCollection:ArrayCollection, defaultValue:String):int
			{
				var len:uint = arrayCollection.length;
				var i:uint = 0;
				var index:int = 0;
				while(i < len) {
					var comboBoxDataP:Object = arrayCollection.getItemAt(i);
					if(comboBoxDataP.data == defaultValue) {
						index = arrayCollection.getItemIndex(comboBoxDataP);
						break;
					}
					i++;
				}
				return index;
			}
			
			private function query(event:MouseEvent):void
			{
				
				var d501Request:D501Request = new D501Request();
				var d501:D501 = new D501();
				//d501.d50102 = this.d501_02.selectedItem.data;
				d501.d50112 = this.d501_12.selectedDate;
				d501.d50112_end = this.d501_12_end.selectedDate;
				/*
				if(this.area.getAreaCode().length == 6) {
					d501.d50114 = this.area.getAreaCode() + "__";
				} else {
					d501.d50114 = this.area.getAreaCode();
				}*/
				
				d501.d50106 = this.d501_06.text;
				d501.d50109 = this.institutionComboBox.getLastSelectedInstitutionCode();
				//d501.d50109 = this.systemUser.institutionCode;
				d501Request.parameterPageindex = 1;
				d501Request.parameterPagesize = 8;
				d501Request.d501 = d501;
				
				if(d501.d50112 != null && d501.d50112_end != null) {
					var r:int = ObjectUtil.compare(d501.d50112, d501.d50112_end);
					if(r == 1) {
						Alert.show("查询起始时间不能大于终止时间!", "系统提示");
						return;
					}
				}
				
				this.certificateApplyService.query(d501Request);
			}
			
			private function queryHandler(event:ResultEvent):void
			{
				var response:Object = event.result;
				if(response.errorMessage == null) {
					
					var type:String = this.institutionComboBox.getLastSelectedInstitution();
					
					switch(type) {
						
						case "6" :
							
							var vd501s:ArrayCollection = response.vd501s;
							var rowCount:Number = response.rowCount;
							
							this.hospitalVG.visible = true;
							this.hospitalVG.percentWidth = 100;
							this.hospitalVG.height = 250;
							
							this.signVG.visible = false;
							this.signVG.width = 0;
							this.signVG.height = 0;
							
							this.replaceVG.visible = false;
							this.replaceVG.width = 0;
							this.replaceVG.height = 0;
							
							this.changeVG.visible = false;
							this.changeVG.width = 0;
							this.changeVG.height = 0;
							
							this.abandonVG.visible = false;
							this.abandonVG.width = 0;
							this.abandonVG.height = 0;
							
							
							
							vd501s = response.vd501s;
							rowCount = response.rowCount;
							
							this.pagerBarHospital.pageSize = 8;
							this.pagerBarHospital.dataGrid = this.HospitalDataGrid;
							this.pagerBarHospital.resultData = vd501s;
							this.pagerBarHospital.rowCount = rowCount;
							this.pagerBarHospital.pagerFunction = pagerFunctionHospital;
							this.pagerBarHospital.dataBind();
							if(vd501s.length <= 0) {
								this.pagerBarHospital.enabled = false;
							} else {
								this.pagerBarHospital.enabled = true;
							}
							break;
						case "8" :
							
							this.hospitalVG.visible = false;
							this.hospitalVG.width = 0;
							this.hospitalVG.height = 0;
							
							this.signVG.visible = true;
							this.signVG.percentWidth = 100;
							this.signVG.height = 250;
							
							this.replaceVG.visible = true;
							this.replaceVG.percentWidth = 100;
							this.replaceVG.height = 250;
							
							this.changeVG.visible = true;
							this.changeVG.percentWidth = 100;
							this.changeVG.height = 250;
							
							this.abandonVG.visible = true;
							this.abandonVG.percentWidth = 100;
							this.abandonVG.height = 250;
							
							this.vd501 = new VD501();
							vd501.d50102 = this.institutionComboBox.getLastSelectedInstitutionCode();
							this.townDetail(vd501);
							break;
					}
				} else {
					Alert.show(response.errorMessage, "系统异常");
					return;
				}
			}
			
