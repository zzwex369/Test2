						</mx:GridItem>
						
					</mx:GridRow>
					
				
					<mx:GridRow visible="{age==2}" id="E304_84Row"
								includeInLayout="{age==2}">
						<mx:GridItem enabled="{age==2}">
							<s:Label styleName="label" text="光照反应"/>
						</mx:GridItem>
						
						<mx:GridItem id="E304_84" enabled="{age==2}">
							
							<s:RadioButton fontSize="10"  label="正常" buttonMode="true" change="E304_85.text=''" styleName="label"
										   groupName="E304_84" value="3"/>
							<s:RadioButton fontSize="10"  id="E304_84_2" label="异常" buttonMode="true" styleName="label"
										   groupName="E304_84" value="2"/>	
