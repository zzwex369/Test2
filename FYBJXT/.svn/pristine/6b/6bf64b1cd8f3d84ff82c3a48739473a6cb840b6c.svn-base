	<fx:Script>
		<![CDATA[
			import com.xyw.module.etbj.model.E304;
			import com.xyw.sys.custom.component.CompMethod;
			import com.xyw.sys.event.CustomDataEvent;
			
			import mx.controls.Alert;
			import mx.events.CloseEvent;
			import mx.events.FlexEvent;
			import mx.messaging.AbstractConsumer;
			import mx.utils.StringUtil;
			
			import org.hamcrest.object.nullValue;
			
			import spark.events.IndexChangeEvent;
			[Bindable]
			public var age:int=0;//月龄
			public var diff:int;
			[Bindable]
			public var ve304:Object;
			private var oldHeight:Number=0;
			private var oldGridRow:Array;
			private var e304:E304;
			private var isFirst:Boolean=false;//第一次派发变化事件
			private var z30406:Array=[null,'0','1','3','6','8','12','14','16','18','20','24','28','32'];
			private var exceptionField:Array=["78","84","86","88","90","92","94"];
			private var explainField:Array=["眼外观 ","光照反应","瞬目反射","红球试验","眼位检查","眼球运动","视物行为观察"];
			public function creationCompleteHandler(event:FlexEvent):void
			{		
					ve304?loadE304SL():setHeight();
			}
			
			/**
			 *
			 */
			private function loadE304SL():void
			{
				
				slChange(ve304.e30402);
				
				if(!ve304.e30498)
					return ;
				E304_80.text=ve304.e30480;
				E304_81.text=ve304.e30481;
				E304_82.text=ve304.e30482;
				E304_83.text=ve304.e30483;
				E304_85.text=ve304.e30485;
				E304_87.text=ve304.e30487;
				E304_89.text=ve304.e30489;
				E304_91.text=ve304.e30491;
				E304_93.text=ve304.e30493;
				E304_96.text=ve304.e30496;
				E304_97.text=ve304.e30497;
				
				CompMethod.setRadioButtonSelect(E304_78,ve304.e30478);
				CompMethod.setRadioButtonSelect(E304_84,ve304.e30484);
				CompMethod.setRadioButtonSelect(E304_86,ve304.e30486);
				CompMethod.setRadioButtonSelect(E304_88,ve304.e30488);
				CompMethod.setRadioButtonSelect(E304_90,ve304.e30490);
				CompMethod.setRadioButtonSelect(E304_92,ve304.e30492);
				CompMethod.setRadioButtonSelect(E304_94,ve304.e30494);
				CompMethod.setRadioButtonSelect(E304_98,ve304.e30498);
				CompMethod.setRadioButtonSelect(E304_99,ve304.e30499);
			
				CompMethod.setCheckBoxAutomationName(E304_95,ve304.e30495);
				CompMethod.setCheckBoxAutomationName([E304_79_1,E304_79_2,E304_79_4,E304_79_4],ve304.e30479);
				
				CompMethod.setRadioButtonSelect(saveHg,"1");

			}
			
			public function slChange(item:Object):void
			{
				if(!item)
					return;
				for(var i:int=0;i<z30406.length;i++)
				{
					if(item.toString()==z30406[i])
					{
						age=i;
						callLater(setHeight);
						break;
					}
				}
			}
			
			/**
			 *
			 */
			private function setHeight():void
			{
				initGrid();
				if(!oldHeight)
					oldHeight=scVgroup.height;
				if(oldHeight!=scVgroup.height||!isFirst)
				{
					//diff=(scVgroup.height)-oldHeight;
					scNav.height=scVgroup.height;
					this.height=scVgroup.height+34;
					isFirst=!isFirst;			
					dispatchEvent(new Event("resizeSldj"));
				}
				oldHeight=scVgroup.height;
			}
			
			
			
			/**
			 *
			 */
			private function initGrid():void
			{
				if(!saveHg_1.selected)
					return ;
				var isAdd:Boolean=false;
				var visible:Boolean=false;
				if(!oldGridRow)
				{
					isAdd=true;						
					oldGridRow=new Array();
				}
				for(var i:uint=0;i<grid.numElements;i++)
				{	
					var child:Object=grid.getElementAt(i);
					if(child.id)//需要匹配的id
					{
						visible=child.visible;
						if(isAdd)//初始化使用
							oldGridRow.push(visible);//对应是否显示
						else//验证是否和原来的是否一致
						{
							if(visible&&visible!=oldGridRow[i])
							{
								var item:Object=child.getElementAt(1);
								CompMethod.setRadioButtonSelect(item,'3');//恢复系统初始化
							}else if(!visible)
								CompMethod.hasNumEles(child);
							oldGridRow[i]=visible;
						}
					}
				}
			}
			
			
			private function checkbox_changeHandler(event:Event,text:TextInput):void
			{
				
				if(!event.target.selected)
					text.text='';
			}
			
			
			
			/**
			 *验证 录入异常是否与结果一致
			 * return true 保存
			 * 		  false  不保存
			 */
			private function validateE30498():void
			{
				var field:String;
				var countArr:Array=[0,0,"",""];//0 异常  1正常  2 异常选项 3 正常选项
				var e30498:String=e304.e30498;
				for(var i:int=0;i<exceptionField.length;i++)
				{
					field=e304["e304"+exceptionField[i]];
					if(field=="2")
					{
						++countArr[0];
						countArr[2]=!countArr[2]?explainField[i]:countArr[2];
					}else if(field=="3")
					{
						++countArr[1];
						countArr[3]=!countArr[3]?explainField[i]:countArr[3];
					}
				}
				
				var idx:int=0;
				if(countArr[0]&&e30498=="3")
					idx=2;
				else if(!countArr[0]&&countArr[1]&&e30498=="2")
					idx=3;
				if(idx)
				{
					var sysResult:String=idx==2?"异常":"正常";
					showWarning(countArr[idx]+"选项与检查结果不一致，是否使用系统自动更改检查结果为"+sysResult+"？"
						,0,E304_98,["e30498",idx.toString()]);
				}
				else
					doShowWarning();
				
			}
			
			/**
			 * text 提示信息
			 * flag 0 下个验证  1 保存
			 * id 组件id 
			 * valArr 0  字段  1字段指定值
			 */
			private function showWarning(text:String,flag:Number=0,id:Object=null,valArr:Array=null):void
			{
				Alert.show(text,"系统警告"
					,3,this,function (event:CloseEvent):void
					{
						
						if(event.detail==1)
						{
							e304[valArr[0]]=valArr[1];
							CompMethod.setRadioButtonSelect(id,valArr[1]);
							doShowWarning();
							
						}
					});
			}
			
			
			/**
			 *检查结果与处理 匹配结果
			 */
			private function doShowWarning(idx:Number=-1):void
			{

				if(e304.e30498=='2'&&e304.e30499!='3')
					showWarning("检查结果为异常，处理不为转诊，是否使用系统自动更改处理结果为转诊？",1,E304_99,["e30499",3]);
					
				else if(e304.e30498=='3'&&e304.e30499=='3')
					showWarning("检查结果为正常，处理为转诊，是否继续保存为定期检查？",1,E304_99,["e30499",1]);
				else
					dispatchCustomEvent();
			}
			
			
			/**
			 *
			 */
			private function dispatchCustomEvent():void
			{
				var event:CustomDataEvent=new CustomDataEvent(CustomDataEvent.CUSTOM_DATA);
				event.data=e304;
				this.dispatchEvent(event);
			}
			/**
			 *
			 */
			public function verifica(e304:E304):void
			{
				this.e304=e304;
				if(CompMethod.getRadioButtonValue(saveHg)=='2')//选择否
				{
					dispatchCustomEvent();
					return ;					
				}
				var resultArr:Array;
				var checkBoxArr:Array=new Array(2);//1眼外观   2视物行为 
				if(E304_78_2.selected)
				{
					
					resultArr=CompMethod.getCheckBoxAutomationName([E304_79_1,E304_79_2,E304_79_3,E304_79_4]);
					if(!resultArr[0])
					{
						Alert.show("请选择眼外观异常","系统提示");
						return;
					}
					if(E304_80.enabled&&!E304_80.text)
					{
						Alert.show("请输入眼外观眼睑异常原因 ","系统提示");
						return;
					}
					if(E304_81.enabled&&!E304_81.text)
					{
						Alert.show("请输入眼外观 结膜异常原因 ","系统提示");
						return;
					}
					if(E304_82.enabled&&!E304_82.text)
					{
						Alert.show("请输入眼外观 角膜异常原因 ","系统提示");
						return;
					}
					if(E304_83.enabled&&!E304_83.text)
					{
						Alert.show("请输入眼外观 瞳孔异常原因 ","系统提示");
						return;
					}
					checkBoxArr[0]=resultArr[1];
				}
				
				if(E304_94_2.selected)
				{
					
					resultArr=CompMethod.getCheckBoxAutomationName(E304_95);
					if(!resultArr[0])
					{
						Alert.show("请选择视物行为观察异常","系统提示");
						return;
					}
					if(E304_96.enabled&&!E304_96.text)
					{
						Alert.show("请输入视物行为观察其他异常","系统提示");
						return;
					}
					
					
					checkBoxArr[1]=resultArr[1];
				}
				
				if(E304_85.enabled&&!E304_85.text)
				{
					Alert.show("请输入光照反应 异常原因 ","系统提示");
					return;
				}
				
				if(E304_87.enabled&&!E304_87.text)
				{
					Alert.show("请输入瞬目反射 异常原因 ","系统提示");
					return;
				}
				
				if(E304_89.enabled&&!E304_89.text)
				{
					Alert.show("请输入红球试验 异常原因 ","系统提示");
					return;
				}
				
				if(E304_91.enabled&&!E304_91.text)
				{
					Alert.show("请输入眼位检查 异常原因 ","系统提示");
					return;
				}
				
				if(E304_93.enabled&&!E304_93.text)
				{
					Alert.show("请输入眼球运动 异常原因 ","系统提示");
					return;
				}
				
				saveE304SL(e304,checkBoxArr);
			}
			
			/**
			 *
			 */
			private function saveE304SL(e304:E304,checkBoxArr:Array):void
			{
				
				//1眼外观   2视物行为 
				e304.e30479=checkBoxArr[0];
				e304.e30495=checkBoxArr[1];
				e304.e30480=StringUtil.trim(E304_80.text);
				e304.e30481=StringUtil.trim(E304_81.text);
				e304.e30482=StringUtil.trim(E304_82.text);
				e304.e30483=StringUtil.trim(E304_83.text);
				e304.e30485=StringUtil.trim(E304_85.text);
				e304.e30487=StringUtil.trim(E304_87.text);
				e304.e30489=StringUtil.trim(E304_89.text);
				e304.e30491=StringUtil.trim(E304_91.text);
				e304.e30493=StringUtil.trim(E304_93.text);
				e304.e30496=StringUtil.trim(E304_96.text);
				e304.e30497=StringUtil.trim(E304_97.text);
				
				e304.e30478=CompMethod.getRadioButtonValue(E304_78);
				e304.e30484=CompMethod.getRadioButtonValue(E304_84);
				e304.e30486=CompMethod.getRadioButtonValue(E304_86);
				e304.e30488=CompMethod.getRadioButtonValue(E304_88);
				e304.e30490=CompMethod.getRadioButtonValue(E304_90);
				e304.e30492=CompMethod.getRadioButtonValue(E304_92);
				e304.e30494=CompMethod.getRadioButtonValue(E304_94);
				e304.e30498=CompMethod.getRadioButtonValue(E304_98);
				e304.e30499=CompMethod.getRadioButtonValue(E304_99);
				validateE30498();
			
			}
			
			private function saveHg_1_changeHandler(event:Event):void
			{
				oldGridRow=new Array(7);
				initGrid();
				E304_97.text="无";
				CompMethod.setRadioButtonSelect(E304_98,'3');
				CompMethod.setRadioButtonSelect(E304_99,'1');
				
			}
			
		]]>
	</fx:Script>
		<s:NavigatorContent id="scNav"  label="筛查情况">
