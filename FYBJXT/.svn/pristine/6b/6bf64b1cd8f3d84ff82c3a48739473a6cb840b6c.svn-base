						</mx:GridItem>
						
						
						<mx:GridItem colSpan="4">
							<s:TextInput id="E304_91" width="100%" enabled="{E304_90_2.selected}" maxChars="100" prompt="最多输入100个字符"/>
						</mx:GridItem>
