			public function saveF303Handler(event:ResultEvent):void
			{
				var f303Response:Object =event.result;
				if(!f303Response.state){
					saveF303.enabled=true;
					Alert.show(f303Response.errorMessage,"系统提示");
					return;
				}
				Alert.show(f303Response.promptMessage+",点击确定即将刷新页面！","系统提示",1,this,closeHander);
