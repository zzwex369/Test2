			public function loadF301(vf301:Object):void
			{
				this.f303.f30302 =vf301.f30101;
				this.f303.f30303 =0;
				this.F303_05.text = vf301.f30102;  //产妇姓名 
				this.F303_07.text = vf301.f30105;  //证件号码     F302_05
				//				this.f303.f30304 = vf301.f30117; // 建册编号
				//				this.F303_127.text = vf301.f30147; // 孕产妇档案编号(济源) 
				this.F303_04.text = vf301.f30117; // 孕产妇档案编号(济源) 
				//证件类型     F302_04
				this.F303_06.selectedIndex =this.getDroupDownListSelectedIndex(ArrayCollection(this.F303_06.dataProvider),vf301.f30104);
				
				this.F303_08.text =vf301.f30106Str;
				this.F303_124.selectedIndex =this.getDroupDownListSelectedIndex(ArrayCollection(this.F303_124.dataProvider),vf301.f30108);
				setMontherBirthday(null);
				this.saveF303.enabled =true;
			}
			private function setMontherBirthday(event:Event):void
			{
				if(this.F303_06.selectedItem.data == 1){
					var cardId:String = this.F303_07.text;
					if(cardId.length>=16){
						cardId=cardId.substring(6,14);
						this.F303_08.text = cardId.substring(0,4) + "-" + cardId.substring(4,6) + "-" + cardId.substring(6,8);
					}else{
						cardId=cardId.substring(6,12);
						this.F303_08.text = "19" + cardId.substring(0,2) + "-" + cardId.substring(2,4) + "-" + cardId.substring(4,6);
					}
				}
			}
			//验证信息
			public function validate1():void
