			public function validate1():void{
				if(this.F401_02.text ==""){
					Alert.show("请输入女性的姓名","系统提示");
					return;
				}
				if(this.F401_03.selectedItem.data ==0){
					Alert.show("请选择女性性别！","系统提示");
					return;
				}
				if(this.F401_05.selectedItem.data ==0){
					Alert.show("请选择女性证件类型","系统提示");
					return;
				}
				if(this.F401_06.text ==""){
					Alert.show("请输入女性的证件号码","系统提示");
					return;
				} 
				var dateValidation:RegExp = /^\d{4}-\d{2}-\d{2}$/g;
				if(this.F401_04.text ==""){
					Alert.show("请输入女性的出生日期","系统提示");
					return;
				}else{
					if(!dateValidation.test(this.F401_04.text)){
						Alert.show("请输入正确的日期格式:XXXX-XX-XX","系统提示");
						return;
					}
				}
				if(this.F401_08.selectedItem.data ==0){
					Alert.show("请选择女性国籍！","系统提示");
					return;
				} 
				if(this.F401_05.selectedItem.data == 1 && this.F401_08.selectedItem.data == "CHN" &&
					!CustomRegex.testRegExp(0,F401_06))
				{	//只有在证件类别为身份证，国籍为中国的时候才验证身份证格式是否正确
					Alert.show("女性的证件号码有误!","系统提示");
					return;
				}
				if(this.F401_12.text ==""){
					Alert.show("请输入女性联系方式！","系统提示");
					return;
				}
				if(this.F401_09.text == ""){
					Alert.show("请输入女性身份证地址!","系统提示");
					return;
				}
				if(this.F401_13.text ==""){
					Alert.show("请输入配偶的姓名","系统提示");
					return;
				}
				if(this.F401_94.text ==""){
					Alert.show("请输入配偶的证件号码","系统提示");
					return;
				}
				if(this.F401_95.selectedItem.data ==0){
					Alert.show("请选择检查单位","系统提示");
					return;
				}
				if(this.F401_93.text ==""){
					Alert.show("请输入检查日期（时）！","系统提示");
					return;
				}
				if(F401_10.getAreaCode().substr(0,4)!="4113")
				{
					Alert.show("现地址请选择南阳市地址","系统提示");
					return ;
				}
				if(!CompMethod.validateAreaCode(F401_10.getAreaCode(),'4113'))
				{
					Alert.show("现地址选择到最下级","系统提示");
					return ;
				}
				saveRecord();
			}
