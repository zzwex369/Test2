			public function saveF401Handle(event:ResultEvent):void{
				var f401Response:Object =event.result;
				f40101= f401Response.f40101
				if(!f401Response.state){
					Alert.show(f401Response.errorMessage,"系统提示");
					return;
				}
				//Alert.show("保存成功！ ","系统提示");
				Alert.show("		保存成功！\n请点击确认之后选择相应功能！ ","系统提示",4,this,picLoad);
				this.clean();	
				this.saveF401.enabled =false;
				this.F401_02.addEventListener(FlexEvent.CHANGING,openSave);
			}
			public function picLoad(e:CloseEvent):void{
				if(e.detail==Alert.OK){
					var womanTitleWindow:WomanTitleWindow = new WomanTitleWindow();
					womanTitleWindow.f40101 = this.f40101;
					womanTitleWindow.diqucode = this.systemUser.institutionCode;
					womanTitleWindow.owner = this;
					PopUpManager.addPopUp(womanTitleWindow, this,true);
					var x:Number = (this.parentApplication.width - womanTitleWindow.width) / 2;
					var y:Number = (this.parentApplication.height - womanTitleWindow.height) / 2;
					womanTitleWindow.move(x,y)
				}
			}
