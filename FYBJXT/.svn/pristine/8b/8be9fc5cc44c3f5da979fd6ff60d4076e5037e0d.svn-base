<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>换发打印设置</title>
	<style type="text/css">
		.td_font
		{
        	font-size:12px;
		}
		@media print {
   			.Noprint {display:none;}
		}
	</style>
	<script type="text/javascript" src="../js/jquery-1.10.2.js"></script>
	<script type="text/javascript" src="../js/printSetupHf.js"></script>
</head>
<body bgcolor="#ffffff">
	<br/>
		<table cellpadding="1" cellspacing="0" border="0" height="20"
				align="center">
			<tr>
				<td width="60" align="center" class="td_font">打印设置</td>
				<td width="60" align="center" class="td_font">打印项：</td>
				<td>
					<select id="printinfo" style="width:80px">
						<option value="">
							请选择
						</option>
					</select>
				</td>
				<td width="60" class="td_font" align="center">显示项：</td>
				<td>
					<select id="birthinfo" style="width:100px">
						<option value="">
							请选择
						</option>
					</select>
					<input type="button" id="addButton" value="添加" title="添加"/>
				</td>
				<td width="20">
					<input type="button" id="delButton" value="删除当前所选" title="删除当前所选" />
				</td>
				<td class="td_font">
					<input type="button" id="saveButton" value="保存"/>
				</td>
			</tr>
		</table>
</body>
</html>