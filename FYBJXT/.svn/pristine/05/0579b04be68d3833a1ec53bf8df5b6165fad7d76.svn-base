			private function vgroup_creationCompleteHandler(event:FlexEvent):void
			{
				resize.play();
				//vgroupDataGrid.width=500;
				addVgroupDataGrid.push(vgroup.height);
				addVgroupDataGrid.push(navigatorContent.height);
				addVgroupDataGrid.push(tabNavigator.height);
				addVgroupDataGrid.push(vgroupDataGrid);
				vgroup.removeElement(vgroupDataGrid);
				callLater(setHeight,new Array(90,90,127));
				
			}
			
			
			private function setHeight(vgroupHeight:Number,navigatorContentHeight:Number,tabNavigatorHeight:Number):void
			{
				vgroup.height=vgroupHeight;
				navigatorContent.height=navigatorContentHeight;
				tabNavigator.height=tabNavigatorHeight;
			}
			
			
			private function readCard():void
			{
				var arr:Array =CompMethod.readCard();
				if(arr)
				{
					if(CompMethod.sex('女',arr[1]))
					{
						L301_02.text=arr[0];
						L301_04.text=arr[5];
						queryL301();
					}else
						Alert.show("请读女性身份证","系统提示");
				}else
					Alert.show("读卡失败","系统提示");
			}
			
			
			
