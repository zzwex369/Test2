package com.xyw.sys.custom.component
{
	/**
	 * 正则类
	 */
	public class CustomRegex
	{
		/**
		 * 通用身份证正则
		 */
		public var commonCardRegExp:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
		
		/**
		 * 手机号码
		 */
		public var cellPhoneRegExp:RegExp = /^1(3|4|5|6|7|8|9)\d{9}$/;
		
		/**
		 * 验证固话号码
		 */
		public var telephoneRegExp:RegExp = /^(0\\d{2}-\\d{8}(-\\d{1,4})?)|(0\\d{3}-\\d{7,8}(-\\d{1,4})?)$/;
		
		
	}
}