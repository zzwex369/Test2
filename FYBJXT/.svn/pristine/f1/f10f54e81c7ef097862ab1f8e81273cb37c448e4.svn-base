			private function moduleCreationComplete():void
			{
				this.systemUser = this.parentApplication.systemUser;
				if(this.systemUser.duka == "1"){
					 this.duka1.enabled = true;
					/* this.duka3.enabled = true;  */
				}
				this.d306_23.text = DateField.dateToString(new Date, "YYYY-MM-DD");
				this.initializeComboBox();
			}
			
			private function initializeComboBox():void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select t.d101_01, t.d101_02 from D101 t";
				comboBoxDataRequest.flag = "D101";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
				
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select * from S301_01";
				comboBoxDataRequest.flag = "S301_01";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
				
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select * from S401_01";
				comboBoxDataRequest.flag = "S401_01";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
				
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select * from S301_06";
				comboBoxDataRequest.flag = "S301_06";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
				
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select * from S301_07";
				comboBoxDataRequest.flag = "S301_07";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
				
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = false;
				comboBoxDataRequest.sql = "select t.s_01,t.s_02 from S301_10 t where t.S_05='0' and t.S_03 ="+this.systemUser.institutionCode;
				comboBoxDataRequest.flag = "S301_10";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			/*-----------------返回数据放到赋值到下拉菜单-----------------*/
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				
				if(flag == "D101")
				{
					var index:int = this.getDropDownListSelectedIndex(comboBoxDataList, this.systemUser.institutionCode);
					this.d306_22.dataProvider = comboBoxDataList;
					this.d306_21.dataProvider = comboBoxDataList;
					this.d306_21.selectedIndex = 0;
					this.d306_22.selectedIndex = index;
				}else if(flag == "S301_01")
				{
					this.d306_05.dataProvider = comboBoxDataList;
					this.d306_05.selectedIndex = 1;
				}else if(flag == "S401_01")
				{
					/* this.d306_13.dataProvider = comboBoxDataList;
					this.d306_13.selectedIndex = 0; */
				}else if(flag == "S301_06")
				{
					var d30607Index:int = this.getDropDownListSelectedIndex(comboBoxDataList, "1");
					this.d306_07.dataProvider = comboBoxDataList;
					this.d306_07.selectedIndex = d30607Index;
					this.d306_11.dataProvider = comboBoxDataList;
					this.d306_11.selectedIndex = d30607Index;
				}else if(flag == "S301_07")
				{
					this.d306_10.dataProvider = comboBoxDataList;
					this.d306_10.selectedIndex = 0;
				}
				
				this.d306_05.selectedIndex = 0;
				this.d306_13.selectedIndex = 0;
			}
			/*-----------验证婴儿信息-------------*/
			private function validateBirth():void
			{
				var queryBirthRequest:QueryBirthRequest = new QueryBirthRequest();
				if(StringUtil.trim(this.d301_02_.text) == "")
				{
					Alert.show("请输入婴儿姓名","系统提示");
					return;
				}else if(StringUtil.trim(this.d301_10_.text) == "")
				{
					Alert.show("请输入婴儿出生证号","系统提示");
					return;
				}
				queryBirthRequest.d301_02 = StringUtil.trim(this.d301_02_.text);
				queryBirthRequest.d301_10 = StringUtil.trim(this.d301_10_.text);
				this.birthService.validateBirth(queryBirthRequest);
			}
			
			/*--------------验证婴儿信息返回----------------*/
			private function validateBirthHandler(event:ResultEvent):void
			{
				var queryBirthResponse:Object = event.result;
				if(queryBirthResponse.state)
				{
					vd301 = queryBirthResponse.vd301;
					this.d306_03.text = vd301.d30110;
					this.d306_04.text = vd301.d30102;
					this.d306_05.selectedIndex =this.getDropDownListSelectedIndex(ArrayCollection(this.d306_05.dataProvider),vd301.d30103);
					this.d306_06.text =vd301.d30111;	
					this.d306_07.selectedIndex =this.getDropDownListSelectedIndex(ArrayCollection(this.d306_07.dataProvider),vd301.d30134);
					this.d306_08.text =vd301.d30112;
					this.d306_09.text =vd301.d30136;
					this.d306_10.selectedIndex =this.getDropDownListSelectedIndex(ArrayCollection(this.d306_10.dataProvider),vd301.d30137);
					this.d306_11.selectedIndex =this.getDropDownListSelectedIndex(ArrayCollection(this.d306_11.dataProvider),vd301.d30138);
					this.d306_12.text =vd301.d30139;
					this.d306_21.selectedIndex =this.getDropDownListSelectedIndex(ArrayCollection(this.d306_21.dataProvider),vd301.d30124);
					this.d306.d30602 =vd301.d30101;
					d30142_type = vd301.d30142;
				}else{
					Alert.show(queryBirthResponse.errorMessage.toString(),"系统提示");
					d30142_type="";
				}
				
			}
			
			private function getDropDownListSelectedIndex(arrayCollection:ArrayCollection, defaultValue:String):int
			{
				var len:uint = arrayCollection.length;
				var i:uint = 0;
				var index:int = 0;
				while(i < len) {
					var comboBoxDataP:Object = arrayCollection.getItemAt(i);
					if(comboBoxDataP.data == defaultValue) {
						index = arrayCollection.getItemIndex(comboBoxDataP);
						break;
					}
					i++;
				}
				return index;
			}
			
			private function validate1():void
			{
				if(StringUtil.trim(this.d306_04.text) == ""){
					Alert.show("请输入婴儿姓名!","系统提示");
					return;
				}
				if(this.d306_05.selectedItem.data == 0){
					Alert.show("请选择性别!","系统提示");
					return;
				}
				if(StringUtil.trim(this.d306_03.text) == ""){
					Alert.show("请输入原出生证号!","系统提示");
					return;
				}
				if(StringUtil.trim(this.d306_06.text) == ""){
					Alert.show("请输入母亲姓名!","系统提示");
					return;
				}
				if(this.d306_07.selectedItem.data == 0){
					Alert.show("请选择母亲证件类型!","系统提示");
					return;
				}
				if(StringUtil.trim(this.d306_08.text) == ""){
					Alert.show("请输入母亲证件号码!","系统提示");
					return;
				}
				if(StringUtil.trim(this.d306_09.text) == ""){
					Alert.show("请输入领证人姓名!","系统提示");
					return;
				}
				if(this.d306_10.selectedItem.data == 0){
					Alert.show("请选择与新生儿关系!","系统提示");
					return;
				}
				if(this.d306_11.selectedItem.data == 0){
					Alert.show("请选择领证人证件类型!","系统提示");
					return;
				}
				if(StringUtil.trim(this.d306_12.text) == ""){
					Alert.show("请输入领证人证件号码!","系统提示");
					return;
				}
				if(this.d306_13.selectedItem.data == 0){
					Alert.show("请选择换发原因","系统提示");
					return;
				}
				if(d30142_type!=="5" && d30142_type=="6" && d30142_type=="7"){
					if(this.d306_21.selectedItem.data == 0){
						Alert.show("请选择助产机构","系统提示");
						return;
					}
				}
				if(this.d306_22.selectedItem.data == 0){
					Alert.show("请选择换发机构","系统提示");
					return;
				}
				if(this.d306_23.text ==""){
					Alert.show("请选择换发日期","系统提示");
					return;
				}
				saveRecord();
			}
			private function saveRecord():void{
				this.d306.d30603 =this.d306_03.text;
				this.d306.d30604 =this.d306_04.text;
				this.d306.d30605 =this.d306_05.selectedItem.data;
				this.d306.d30606 =this.d306_06.text;	
				this.d306.d30607 =this.d306_07.selectedItem.data;
				this.d306.d30608 =this.d306_08.text;
				this.d306.d30609 =this.d306_09.text;
				this.d306.d30610 =this.d306_10.selectedItem.data;
				this.d306.d30611 =this.d306_11.selectedItem.data;
				this.d306.d30612 =this.d306_12.text;
				this.d306.d30613 =this.d306_13.selectedItem.data;
				guide14();
				guide15();
				guide16();
				guide17();
				guide18();
				guide19();
				this.d306.d30620 =this.d306_20.text;
				this.d306.d30621 =this.d306_21.selectedItem.data;
				this.d306.d30622 =this.d306_22.selectedItem.data;
				this.d306.d30623 =DateField.stringToDate(this.d306_23.text,"YYYY-MM-DD");
				this.d306Service.saveD306(d306);
			}
			protected function saveD306Handler(event:ResultEvent):void{
				var d306Response:Object =event.result;
				if(!d306Response.state){
					Alert.show(d306Response.errorMessage,"系统提示");
					return;
				}
				Alert.show("保存成功！ ","系统提示");
			}
			private function guide14():void{
				if(this.D306_14_1.selected){
					array[1401] ="1";
				}else if(this.D306_14_2.selected){
					array[1401] ="2";
				}else{
