			/*--------------根据与婴儿关系加载领证人信息-----------------*/
			private function changeLicen():void
			{
				var id:String = this.d306_10.selectedItem.data;
				if(id == "2")
				{	
					this.d306_09.text = this.d306_06.text;
					this.d306_12.text = this.d306_08.text;
					this.d306_11.selectedIndex = this.d306_07.selectedIndex;
				}else
				{
					this.d306_09.text = "";
					this.d306_12.text = "";
					this.d306_11.selectedIndex = 1;
				}
			}
			//读卡(母亲)
			protected function readCard_mother(event:MouseEvent):void
			{
				try
				{
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						var array:Array = str.split(",");
						var sex:String = array[1];//性别
						if(sex=="女"){
							this.d306_06.text = array[0];//姓名
							this.d306_08.text = array[5];//身份证号
						}else{
							Alert.show("请读母亲信息！","系统提示");
						}
					}else{
						Alert.show("读卡失败！");
					}
				} 
				catch(error:Error) 
				{
					Alert.show("读卡失败！");
				}
			}
			//与新生儿关系（读卡）
			private function readCard_A(event:MouseEvent):void{
				try
				{
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						var array:Array = str.split(",");
						this.d306_09.text = array[0];//领证人姓名
						this.d306_12.text = array[5];//领证人身份证号
					}else{
						Alert.show("读卡失败！");
					}
				} 
				catch(error:Error) 
				{
					Alert.show("读卡失败！");
				}
			}
		]]>
	</fx:Script>
	<s:Scroller width="100%" height="100%" horizontalScrollPolicy="auto" verticalScrollPolicy="auto">
