			public var systemUser:SystemUser = null;
			public var vd301:Object = null;
			public var d30142_type:String ="";
			
			private var d306_13data:ArrayCollection = new ArrayCollection([
				{label:"选择",data:0},
				{label:"变更姓名",data:6},
