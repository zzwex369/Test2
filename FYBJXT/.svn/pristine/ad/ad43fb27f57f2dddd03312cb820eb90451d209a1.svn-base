				}
				else if(this.E504_19_02.selected){
					array[902] = '2';
					e504.e50419 = array[902];
				}
			}
			
			
			/**
			 *读卡
			 */
			private function readCard():void
			{
				try
				{
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						var array:Array = str.split(",");
						var sex:String = array[1];//性别
						if(sex=="女"){
							this.E504_06.text = array[0];//姓名
							this.E504_07.text = array[5];//身份证号
							this.E504_08.text = array[4];//身份证地址
						}else{
							Alert.show("请读母亲信息！","系统提示");
						}
					}else{
						Alert.show("读卡失败！");
					}
				}catch(error:Error) 
				{
					Alert.show("读卡失败！");
				}
			}
			
