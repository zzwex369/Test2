<?xml version="1.0" encoding="utf-8"?>
<s:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx" 
		  xmlns:component = "com.xyw.sys.custom.component.*"
		  width="100%" height="100%" creationComplete="creationCompleteHandler(event)">
	<fx:Declarations>
		<s:RemoteObject id="systemService" destination="systemService"
						endpoint="{this.parentApplication.contextRoot}/messagebroker/amf"
						showBusyCursor="true">
			<s:method name="getComboBoxData" result="getComboBoxDataHandler(event)"/>
		</s:RemoteObject>
		<s:RemoteObject id="sw301Service" destination="sw301Service"
						endpoint="{this.parentApplication.contextRoot}/messagebroker/amf"
						showBusyCursor="true">
			<s:method name="saveSw301" result="saveSw301Handler(event)"/>
		</s:RemoteObject>
	</fx:Declarations>
	
	<fx:Style>
		@namespace s "library://ns.adobe.com/flex/spark";
		@namespace mx "library://ns.adobe.com/flex/mx";
		mx|GridItem
		{
			horizontalAlign:left;
			verticalAlign:middle;
		}
		
		s|DropDownList
		{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomDropDownListSkin");
			selectionColor:#DDDDDD;
			rollOverColor:#EEEEEE;	
		}
		s|Button
		{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomButtonSkin");
		}
		s|Label
		{
			fontSize:12;
		}
		s|DataGrid
		{
			borderAlpha:"0.3";
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomDataGridSkin");
			alternatingRowColors:"[#FFFFFF,#EEEEEE]";
		}
		s|TextInput
		{
			fontFamily:"微软雅黑";
		}
		
	.must {
			color:red;
			fontSize:20px;
			fontWeight:bold;
			paddingTop:5px;
		}
	</fx:Style>
	
	<fx:Script>
		<![CDATA[
			import com.xyw.module.swjc.model.Sw301;
			import com.xyw.sys.custom.component.CompMethod;
			import com.xyw.sys.model.ComboBoxDataRequest;
			import com.xyw.sys.model.SystemUser;
			
			import mx.collections.ArrayCollection;
			import mx.controls.Alert;
			import mx.controls.DateField;
			import mx.events.FlexEvent;
			import mx.rpc.events.ResultEvent;
			import mx.utils.StringUtil;
			
			import spark.events.IndexChangeEvent;
			private var sw301:Sw301;
			private var sw30135OldEnabled:Boolean=true;
			private var systemUser:SystemUser = null;
			private var comboBoxDataRequest:ComboBoxDataRequest;
			private var radiobuttonIdArr:Array=null;

			/**
			 *设置户口地址 
			 */
			private function SW301_45_clickHandler(event:MouseEvent):void
			{
				
				SW301_45.setAreaCode(SW301_44.getAreaCode());
				
			}
			
			private function getComboBoxData(sql:String,flag:String,showPrompt:Boolean=true):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = showPrompt;
				comboBoxDataRequest.sql =sql ;
				comboBoxDataRequest.flag = flag;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			/**
			 *
			 */
			private function getComboBoxDatas():void
			{
				this.systemUser = this.parentApplication.systemUser;
				getComboBoxData("select t.d101_01, t.d101_02 from D101 t where t.D101_01 = " +
					this.systemUser.institutionCode ,"D101",false);
				
				getComboBoxData("select s_01,s_02 from S301_01 ","S301_01",false);
				
				getComboBoxData("select s_01,s_02 from S301_06 ","S301_06");
				
				getComboBoxData("select * from SW301_01" ,"SW301_01",false);
				
				getComboBoxData("select * from SW301_04" ,"SW301_04");
			}
			
			private function SW301_30_changeHandler(event:IndexChangeEvent):void
			{
				getComboBoxData("select sw_03,sw_02 from SW301_05 where sw_01="+SW301_30.selectedItem.data,"SW301_05");
			}
			
			
			private function creationCompleteHandler(event:FlexEvent):void
			{
				getComboBoxDatas();
				radiobuttonIdArr=[
					[SW301_20Explain,SW301_20Label,SW301_20Row]	,
					[SW301_32Explain,SW301_32Label,SW301_32Row]	
				];
			}
			
			/**
			 *
			 */
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				if(flag == "D101")
				{
					this.SW301_43.dataProvider = comboBoxDataList;
					this.SW301_43.selectedIndex = 0;	
				}else if(flag=="S301_01")
				{
					SW301_15.dataProvider=comboBoxDataList;
					SW301_15.selectedIndex=0;
				}else if(flag=="S301_06")
				{
					SW301_08.dataProvider=comboBoxDataList;
					SW301_11.dataProvider=comboBoxDataList;
					SW301_08.selectedIndex=1;
					SW301_11.selectedIndex=0;

				}else if(flag=="SW301_01")
				{
					SW301_14.dataProvider=comboBoxDataList;
					SW301_14.selectedIndex=0;
					
				}else if(flag=="SW301_04")
				{
					SW301_30.dataProvider=comboBoxDataList;
					SW301_30.selectedIndex=0;
					
				}else if(flag=="SW301_05")
				{
					SW301_31.dataProvider=comboBoxDataList;
					SW301_31.selectedIndex=0;
					
				}
			}
			
			
			/**
			 *
			 */
			private function getRadioButton():void
			{
				sw301=new Sw301();
				sw301.sw30120=CompMethod.getRadioButtonValue([SW301_20]);
				sw301.sw30132=CompMethod.getRadioButtonValue([SW301_32]);
				sw301.sw30135=CompMethod.getRadioButtonValue([SW301_35]);
				sw301.sw30137=CompMethod.getRadioButtonValue([SW301_37]);
				sw301.sw30138=CompMethod.getRadioButtonValue([SW301_38]);
				sw301.sw30139=CompMethod.getRadioButtonValue([SW301_39]);
			}
			
			/**
			 *
			 */
			private function validate():void
			{
				
				getRadioButton();
				if(!SW301_07.text)
				{
					Alert.show("请输入母亲姓名","系统提示");
					return;
				}
				if(!SW301_08.selectedIndex)
				{
					Alert.show("请选择母亲证件类型","系统提示");
					return;
				}
				if(!SW301_09.text)
				{
					Alert.show("请输入母亲身份证号","系统提示");
					return;
				}
				if(SW301_08.selectedIndex==1)
				{
					var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
					if(!patternMonther.test(StringUtil.trim(SW301_09.text)))
					{
						Alert.show("母亲身份证号码有误!","系统提示");
						return;
					}
				}
				if(!SW301_13.text)
				{
					Alert.show("请输入联系方式","系统提示");
					return;
				}
				if(SW301_10.text||SW301_11.selectedIndex>0||SW301_12.text)
				{
					if(!SW301_10.text)
					{
						Alert.show("请输入父亲姓名","系统提示");
						return;
					}
					if(!SW301_11.selectedIndex)
					{
						Alert.show("请选择父亲证件类型","系统提示");
						return;
					}
					if(!SW301_12.text)
					{
						Alert.show("请输入父亲身份证号","系统提示");
						return;
					}
					if(SW301_11.selectedIndex==1)
					{
						patternMonther = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
						if(!patternMonther.test(StringUtil.trim(SW301_12.text)))
						{
							Alert.show("父亲身份证号码有误!","系统提示");
							return;
						}
					}
				}
				if(!SW301_16_1.text)
				{
					Alert.show("请输入出生日期(年月日)","系统提示");
					return;
				}
				var hour:Number=Number(SW301_16_2.text);
				if(isNaN(hour))
				{
					Alert.show("请输入出生时间(时)","系统提示");
					return;
				}
				if(hour<0||hour>23)
				{
					Alert.show("出生时间(时)范围在0-23之间","系统提示");
					return;
				}
				var min:Number=Number(SW301_16_3.text);
				if(isNaN(min))
				{
					Alert.show("请输入出生时间（分）","系统提示");
					return;
				}
				if(min<0||min>59)
				{
					Alert.show("出生时间(分)范围在0-23之间","系统提示");
					return;
				}
				
				if(isNaN(Number(SW301_17.text))||!SW301_17.text)
				{
					Alert.show("请输入正确的出生体重","系统提示");
					return;
				}
				if(!SW301_19.text)
				{
					Alert.show("请输入孕周","系统提示");
					return;
				}
				if((sw301.sw30120=='1'||sw301.sw30120=='2')&&
					!SW301_20Explain.text)
				{
					Alert.show("请输入"+SW301_20Label.text,"系统提示");
					return;
				}
				if(!SW301_23_1.text)
				{
					Alert.show("请输入死亡日期(年月日)","系统提示");
					return;
				}
				hour=Number(SW301_23_2.text);
				if(isNaN(hour))
				{
					Alert.show("请输入死亡时间(时)","系统提示");
					return;
				}
				if(hour<0||hour>23)
				{
					Alert.show("死亡时间(时)范围在0-23之间","系统提示");
					return;
				}
				min=Number(SW301_23_3.text);
				if(isNaN(min))
				{
					Alert.show("请输入死亡时间（分）","系统提示");
					return;
				}
				if(min<0||min>59)
				{
					Alert.show("死亡时间(分)范围在0-23之间","系统提示");
					return;
				}
				
				
				if(!SW301_24_1.text||!SW301_24_2.text||
					!SW301_24_3.text||!SW301_24_4.text||
					!SW301_24_5.text)
				{
					Alert.show("请输入死亡年龄，不足填写0","系统提示");
					return;
				}
				if(!SW301_25.text)
				{
					Alert.show("请输入(a)直接导致死亡的疾病或情况","系统提示");
					return;
				}
				if(SW301_28.text&&!SW301_27.text)
				{
					Alert.show("请输入(c)引起(b)的疾病或情况","系统提示");
					return;
				}
				if(SW301_27.text&&!SW301_26.text)
				{
					Alert.show("请输入(b)引起(a)的疾病或情况","系统提示");
					return;
				}
				if(!SW301_29.text)
				{
					Alert.show("请输入根本原因","系统提示");
					return;
				}
				if(!SW301_30.selectedIndex)
				{
					Alert.show("请选择分类编号","系统提示");
					return;
				}
				if(!SW301_31.selectedIndex)
				{
					Alert.show("请选择ICD-10编码","系统提示");
					return;
				}
				if((sw301.sw30132=='1'||sw301.sw30132=='5')&&
					!SW301_32Explain.text)
				{
					Alert.show("请输入"+SW301_32Label.text,"系统提示");
					return;
				}
				if(sw301.sw30135=='1')
				{
					if(!sw301.sw30137)
					{
						Alert.show("请选择死前治疗医院级别 ","系统提示");
						return;
					}
					if(!SW301_36.text)
					{
						Alert.show("请输入死前治疗医院名称","系统提示");
						return;
					}
				}
				
				if((sw301.sw30135=='3'||sw301.sw30138=='5')&&
					!sw301.sw30139)
				{
					Alert.show("请选择未治疗或未就医主要原因 ","系统提示");
					return;
				}
			
				if(!CompMethod.validateAreaCode(SW301_44.getAreaCode(),'4113'))
				{
					Alert.show("现地址选择到最下级","系统提示");
					return ;
				}
				if(!CompMethod.validateAreaCode(SW301_45.getAreaCode(),"4113",true))
				{
					Alert.show("户口地址请选择到乡镇","系统提示");
					return ;
				}
				if(!SW301_41.text)
				{
					Alert.show("请输入录入人员信息","系统提示");
					return ;
				}
				saveSw301();
			}
			
			/**
			 *
			 */
			private function saveSw301():void
			{
				saveSW301.enabled=false;
				sw301.sw30106=StringUtil.trim(SW301_06.text);
				sw301.sw30107=StringUtil.trim(SW301_07.text);
				sw301.sw30108=SW301_08.selectedItem.data;
				sw301.sw30109=StringUtil.trim(SW301_09.text);
				sw301.sw30110=StringUtil.trim(SW301_10.text);
				sw301.sw30111=SW301_11.selectedItem.data;
				sw301.sw30112=StringUtil.trim(SW301_12.text);
				sw301.sw30113=StringUtil.trim(SW301_13.text);
				sw301.sw30114=SW301_14.selectedItem.data;
				sw301.sw30115=SW301_15.selectedItem.data;
				sw301.sw30116=DateField.stringToDate(SW301_16_1.text,'YYYY-MM-DD');
				sw301.sw30116.hours=Number(SW301_16_2.text)
				sw301.sw30116.minutes=Number(SW301_16_3.text)
				sw301.sw30117=StringUtil.trim(SW301_17.text);
				sw301.sw30118=CompMethod.getRadioButtonValue([SW301_18]);
				sw301.sw30119=StringUtil.trim(SW301_19.text);
				if(sw301.sw30120=='1')
					sw301.sw30121=StringUtil.trim(SW301_20Explain.text);
				else if(sw301.sw30120=='2')
					sw301.sw30122=StringUtil.trim(SW301_20Explain.text);
				
				sw301.sw30123=DateField.stringToDate(SW301_23_1.text,'YYYY-MM-DD');
				sw301.sw30123.hours=Number(SW301_23_2.text)
				sw301.sw30123.minutes=Number(SW301_23_3.text)
				sw301.sw30124=SW301_24_1.text+','+SW301_24_2.text+','+
							  SW301_24_3.text+','+SW301_24_4.text+','+
							  SW301_24_5.text;
				sw301.sw30125=StringUtil.trim(SW301_25.text);
				sw301.sw30126=StringUtil.trim(SW301_26.text);
				sw301.sw30127=StringUtil.trim(SW301_27.text);
				sw301.sw30128=StringUtil.trim(SW301_28.text);
				
				sw301.sw30129=StringUtil.trim(SW301_29.text);
				sw301.sw30130=SW301_30.selectedItem.data;
				sw301.sw30131=SW301_31.selectedItem.data;
				
				if(sw301.sw30132=='1')
					sw301.sw30133=StringUtil.trim(SW301_32Explain.text);
				else if(sw301.sw30132=='5')
					sw301.sw30134=StringUtil.trim(SW301_32Explain.text);
				
				sw301.sw30136=StringUtil.trim(SW301_36.text);
				sw301.sw30138=CompMethod.getRadioButtonValue([SW301_38]);
				sw301.sw30140=CompMethod.getRadioButtonValue([SW301_40]);
				sw301.sw30141=StringUtil.trim(SW301_41.text);
				sw301.sw30142=DateField.stringToDate(SW301_42.text,'YYYY-MM-DD');
				sw301.sw30143=SW301_43.selectedItem.data;
				sw301.sw30144=SW301_44.getAreaCode();
				sw301.sw30145=SW301_45.getAreaCode();
				sw301Service.saveSw301(sw301);
			}
			
			/**
			 *
			 */
			private function saveSw301Handler(event:ResultEvent):void
			{
				var sw301Response:Object =  event.result;
				if(!sw301Response.state) {
					saveSW301.enabled=true;
					Alert.show(sw301Response.errorMessage,"系统提示");
					return;
				}
				var thisObj:Object=this;
				Alert.show("保存成功！ ","系统提示",1,this,
					function ():void
					{
						CompMethod._refreshIndex(thisObj);
					}
				);
			}
			
			/**
			 *
			 */
			private function setRowVisible(idx:int,visible:Boolean,label:String):void
			{
				radiobuttonIdArr[idx][0].text='';
				radiobuttonIdArr[idx][1].text=label;
				radiobuttonIdArr[idx][2].visible=visible;
				radiobuttonIdArr[idx][2].includeInLayout=visible;
			}
			
			private function radiobutton_changeHandler(idx:int,visible:Boolean,suffixLabel:String,event:Event):void
			{
				setRowVisible(idx,visible,event.target.label+suffixLabel);
			}
			
			private function SW301_35Radiobutton_changeHandler(enabled:Boolean=false):void
			{
				if(sw30135OldEnabled)
				{
					SW301_36.text='';
					CompMethod._walk(SW301_37);
				}
				sw30135OldEnabled=enabled;
				SW301_36.enabled=enabled;
				SW301_37.enabled=enabled;
				SW301_38radiobutton_changeHandler();
			}
			
			private function SW301_38radiobutton_changeHandler():void
			{
				var SW301_35_3Select:Boolean=SW301_35_3.selected;
				var  SW301_38_8Select:Boolean=SW301_38_5.selected;
				if(!SW301_35_3Select&&!SW301_38_8Select)
					CompMethod._walk(SW301_39);
				
			}
			
			
			
			
		]]>
	</fx:Script>
	
	<s:Scroller x="1" y="-1" width="100%"  horizontalScrollPolicy="auto"
				verticalScrollPolicy="auto" height="100%">
		<s:VGroup paddingBottom="20" paddingLeft="10" paddingRight="10" height="100%">
			<mx:TabNavigator width="1210" chromeColor="#ffffff" creationPolicy="all">
				<s:NavigatorContent width="100%" label="父母基本信息">
					<mx:Grid paddingBottom="10" paddingLeft="8" >
						<mx:GridRow>
							<mx:GridItem>
								<s:Label text="母亲姓名"/>
								<s:TextInput id="SW301_07" width="130" />
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							<mx:GridItem>
								<s:Label paddingLeft="15" text="证件类型" />
								<s:DropDownList id="SW301_08" width="130"/>
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							
							<mx:GridItem>
								<s:Label paddingLeft="15" text="母亲身份证号"/>
								<s:TextInput id="SW301_09" width="179" />
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							
							<mx:GridItem>
								<s:Label text="联系方式"/>
								<s:TextInput id="SW301_13" width="130" />
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							
							
						</mx:GridRow>
						<mx:GridRow>
							<mx:GridItem>
								<s:Label text="父亲姓名"/>
								<s:TextInput id="SW301_10" width="130" />
							</mx:GridItem>
							<mx:GridItem>
								<s:Label paddingLeft="15" text="证件类型" />
								<s:DropDownList id="SW301_11" width="130"/>
							</mx:GridItem>
							
							<mx:GridItem>
								<s:Label paddingLeft="15" text="父亲身份证号"/>
								<s:TextInput id="SW301_12" width="179" />
							</mx:GridItem>
							
						</mx:GridRow>
						
						
						<mx:GridRow>
							<mx:GridItem colSpan="4">
								<s:Label width="57" text="现住址"/>
								<component:AreaComboBoxVillageAuto id="SW301_44" width="948" buttonMode="true" areaCode="4113"/>
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
						</mx:GridRow>
						
						<mx:GridRow>
							<mx:GridItem colSpan="4">
								<s:Label width="57" text="户口地址"/>
								<component:AreaComboBoxVillageAuto id="SW301_45" width="948" areaCode="4113"/>
								<s:Label styleName="must" text="*"/>
								<s:Button label="同现住址" click="SW301_45_clickHandler(event)" buttonMode="true" />
							</mx:GridItem>
						</mx:GridRow>
						
						
					</mx:Grid>  
				</s:NavigatorContent>
			</mx:TabNavigator>
			
			
			<mx:TabNavigator width="1210" chromeColor="#ffffff" creationPolicy="all">
				<s:NavigatorContent width="100%" label="儿童(婴儿)基本信息">
					<mx:Grid paddingBottom="10" paddingLeft="8">
						<mx:GridRow>
							<mx:GridItem>
								<s:Label text="儿童姓名"/>
								<s:TextInput id="SW301_06" width="130" />
								
							</mx:GridItem>
							<mx:GridItem>
								<s:Label paddingLeft="15" text="性别" />
								<s:DropDownList id="SW301_15" width="130"/>
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							
							<mx:GridItem>
								<s:Label paddingLeft="15" text="出生体重"/>
								<s:BorderContainer width="130" height="23">
									<s:HGroup verticalAlign="middle">
										<s:TextInput id="SW301_17" width="100" borderVisible="false" maxChars="3" textAlign="right" restrict="0-9|."/>
										<s:Label text="kg"/>
									</s:HGroup>
								</s:BorderContainer>
								<s:Label styleName="must" text="*"/>
							</mx:GridItem>
							
							<mx:GridItem id="SW301_18">
								<s:Label paddingLeft="15" text="体重测量方式"/>
								<s:RadioButton buttonMode="true" label="测量" groupName="SW301_18" value="1"  selected="true"/>
								<s:RadioButton buttonMode="true" label="估计" groupName="SW301_18" value="2" />
							</mx:GridItem>
							
