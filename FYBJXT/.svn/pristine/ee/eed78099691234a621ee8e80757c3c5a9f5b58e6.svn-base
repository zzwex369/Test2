			private function _initializeComboBox(sql:String,flag:String,showPrompt:Boolean=true):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = showPrompt;
				comboBoxDataRequest.sql =sql ;
				comboBoxDataRequest.flag = flag;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			
			private function initializeComboBox():void
			{
				_initializeComboBox("select t.d101_01, t.d101_02 from D101 t ","D101");
				
				_initializeComboBox("select * from S301_01","S301_01");
				
				_initializeComboBox("select * from S301_03","S301_03");
				
				_initializeComboBox("select * from S301_04","S301_04");
				
				_initializeComboBox("select * from S301_06","S301_06");
				
				_initializeComboBox("select * from S601_10","S601_10");
				
			}
			
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				if(flag == "S301_01")//性别
				{
					this.e301_04.dataProvider = comboBoxDataList;
					this.e301_04.selectedIndex = 0;
				}else if(flag == "S301_04")//民族
				{
					this.e301_08.dataProvider = comboBoxDataList;
					this.e301_08.selectedIndex = 1;
				}else if(flag =="S301_06"){  //证件类型
					var TypeIndex:int =this.getDropDownListSelectedIndex(comboBoxDataList,"1");
					this.F301_04.dataProvider =comboBoxDataList;
					this.F301_04.selectedIndex =TypeIndex;
					this.F301_20.dataProvider =comboBoxDataList;
					this.F301_20.selectedIndex =TypeIndex;
				}else if(flag == "S301_03")//国籍
				{
					var countryIndex:int = this.getCountrySelectedIndex(comboBoxDataList);
					this.e301_07.dataProvider = comboBoxDataList;
					this.e301_07.selectedIndex = countryIndex;
					this.F301_08.dataProvider =comboBoxDataList;
					this.F301_08.selectedIndex =countryIndex;
				}else if(flag == "D101")//单位
				{
					var index:int = this.getDropDownListSelectedIndex(comboBoxDataList, this.systemUser.institutionCode);
					this.e301_16.dataProvider = comboBoxDataList;
					this.e301_16.selectedIndex = index;
					this.e301_21.dataProvider = comboBoxDataList;
					this.e301_21.selectedIndex = index;
					this.e301_33.dataProvider = comboBoxDataList;
					this.e301_33.selectedIndex = index;
					this.e301_16.enabled =false;
				//	this.e301_21.enabled =false;
				}else if(flag == "S601_10")//胎数
				{
					this.e301_20.dataProvider = comboBoxDataList;
					this.e301_20.selectedIndex = 1;
				}
				--count;
				if(!count)
					putCard();
			}
