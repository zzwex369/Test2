				this.F301_08.selectedIndex =f30108Index;
				
				this.e301_03.text =ve301.e30103;
				var e30104dataProvide:ArrayCollection =ArrayCollection(this.e301_04.dataProvider);
				var e30104Index:int =this.getDropDownListSelectedIndex(e30104dataProvide,ve301.e30104);
				this.e301_04.selectedIndex =e30104Index;
				if(ve301.e30106Str !=null){
