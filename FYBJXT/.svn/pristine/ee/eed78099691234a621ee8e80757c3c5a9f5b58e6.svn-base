			/*----------建档单位-----------------*/
			private function getDropDownListSelectedIndex(arrayCollection:ArrayCollection, defaultValue:String):int
			{
				var len:uint = arrayCollection.length;
				var i:uint = 0;
				var index:int = 0;
				while(i < len) {
					var comboBoxDataP:Object = arrayCollection.getItemAt(i);
					if(comboBoxDataP.data == defaultValue) {
						index = arrayCollection.getItemIndex(comboBoxDataP);
						break;
					}
					i++;
				}	
				return index;
			}
			/*-----------------默认国籍 中国---------------------*/
			private function getCountrySelectedIndex(arrayCollection:ArrayCollection):int
			{
				var len:uint = arrayCollection.length;
				var i:uint = 0;
				var index:int = 0;
				while(i < len) {
					var comboBoxDataP:Object = arrayCollection.getItemAt(i);
					if(comboBoxDataP.data == "CHN") {
						index = arrayCollection.getItemIndex(comboBoxDataP);
						break;
					}
					i++;
				}
				return index;
			}
			
			private function validate1():void
			{
				if(StringUtil.trim(this.e301_03.text) == ""){
					Alert.show("请输入儿童姓名!","系统提示");
					return;
				}
				if(this.e301_04.selectedItem.data == 0){
					Alert.show("请选择儿童性别!","系统提示");
					return;
				}
				if(StringUtil.trim(this.e301_06.text) ==""){
					Alert.show("请输入儿童的出生日期","系统提示");
					return;
				}
				if(!CustomRegex.testRegExp(CustomRegex.DATE,StringUtil.trim(e301_06.text)))
				{
					Alert.show("儿童的出生日期格式不正确!","系统提示");
					return;
				}
				if(this.e301_07.selectedItem.data ==0){
					Alert.show("请选择儿童的国籍！","系统提示");
					return;
				}
				if(this.e301_08.selectedItem.data ==0){
					Alert.show("请选择儿童的民族！","系统提示");
					return;
				}
				if(StringUtil.trim(this.e301_30.text) !=""){
					if(Number(this.e301_30.text) >= 200 || Number(this.e301_30.text) <= 10){
						Alert.show("儿童身高应该在20到200cm之间！","系统提示");
						return;
					}
				}
				if(StringUtil.trim(this.e301_19.text) !=""){
					if(Number(this.e301_19.text) >= 100 || Number(this.e301_19.text) <= 1){
						Alert.show("体重应该在1到100之间","系统提示");
						return;
					}
				}
				/* if(this.e301_05.text == ""){
				Alert.show("请输入儿童身份证号!","系统提示");
				return;
				} */ 
				if(StringUtil.trim(this.e301_18_1.text) != ""){
					if(Number(this.e301_18_1.text) > 45 || Number(this.e301_18_1.text) < 20){
						Alert.show("胎龄不能大于45周或者小于20周！","系统提示");
						return;
					}			
				}
				
				if(StringUtil.trim(this.e301_18_2.text) != ""){
					if(Number(this.e301_18_2.text) >7){
						Alert.show("胎龄天数不能大于7！","系统提示");
						return;
					}				
				}
				//if(this.e301_19.text == "0"){
				//Alert.show("请输入儿童的体重！","系统提示");
				//return;
				//}
				if(this.e301_20.selectedItem.data == 0){
					Alert.show("请选择胎数","系统提示");
					return;
				}
				if(StringUtil.trim(this.e301_22.text) !=""){
					if(Number(this.e301_22.text) ==0 || Number(this.e301_22.text) >= 20){
						Alert.show("孕次不合理","系统提示");
						return;
					}
				}
				
				if(StringUtil.trim(this.e301_23.text) !=""){
					if(Number(this.e301_23.text) ==0 || Number(this.e301_23.text) >= 20||Number(this.e301_23.text)>Number(this.e301_22.text)){
						Alert.show("产次不合理","系统提示");
						return;
					}
				}
//				if(StringUtil.trim(this.e301_11.text) == ""){
//					Alert.show("请输入联系电话!","系统提示");
//					return;
//				}
//				if(!CustomRegex.testRegExp(CustomRegex.CELLPHONE,StringUtil.trim(e301_11.text)))
//				{
//					Alert.show("联系电话不正确!","系统提示");
//					return;
//				}
				if(StringUtil.trim(this.e301_17.text) == ""){
					Alert.show("请输入保健卡号!","系统提示");
					return;
				}
				if(this.e301_16.selectedItem.data == 0){
					Alert.show("请选择建档单位","系统提示");
					return;
				}
				if(StringUtil.trim(e301_15.text)=="")
				{
					Alert.show("请输入建档人信息","系统提示");
					return;
				}
				if(StringUtil.trim(this.e301_14.text) ==""){
					Alert.show("请输入建档日期（时）！","系统提示");
					return;
				}
				if(!CustomRegex.testRegExp(CustomRegex.DATE,StringUtil.trim(e301_14.text)))
				{
					Alert.show("建档日期格式不正确!","系统提示");
					return;
				}
				if(StringUtil.trim(e301_34.text)=="")
				{
					Alert.show("请输入录入人员信息","系统提示");
					return;
				}
				this.save();
			}
			public function save():void{
				this.e301.e30103 =StringUtil.trim(this.e301_03.text);
				this.e301.e30104 =this.e301_04.selectedItem.data; 
				this.e301.e30106 =DateField.stringToDate(StringUtil.trim(this.e301_06.text), "YYYY-MM-DD"); 
				this.e301.e30107 =this.e301_07.selectedItem.data;
				this.e301.e30108 =this.e301_08.selectedItem.data;
				this.e301.e30109 =ve301.e30109; //this.e301_09.getAreaCode();
				this.e301.e30111 =StringUtil.trim(this.e301_11.text);
				this.e301.e30112 =ve301.e30112; //this.e301_12.getAreaCode();
				this.e301.e30113 =StringUtil.trim(this.e301_13.text);
				this.e301.e30114 =DateField.stringToDate(StringUtil.trim(this.e301_14.text), "YYYY-MM-DD");   
				this.e301.e30115 =StringUtil.trim(this.e301_15.text);
				this.e301.e30116 =this.e301_16.selectedItem.data; 
				this.e301.e30117 =StringUtil.trim(this.e301_17.text);
				this.e301.e30118 =StringUtil.trim(this.e301_18_1.text) +"," +StringUtil.trim(this.e301_18_2.text);
				this.e301.e30119 =Number(StringUtil.trim(this.e301_19.text));
				this.e301.e30120 =this.e301_20.selectedItem.data; 
				this.e301.e30121 =this.e301_21.selectedItem.data; 
				this.e301.e30122 =Number(StringUtil.trim(this.e301_22.text));
				this.e301.e30123 =Number(StringUtil.trim(this.e301_23.text));
				this.e301.e30125 =this.ve301.e30125;
				this.e301.e30126 =StringUtil.trim(this.e301_26.text);
				this.e301.e30127 =StringUtil.trim(this.e301_27.text);
				this.e301.e30128 =StringUtil.trim(this.e301_28.text);
				this.e301.e30129 =StringUtil.trim(this.e301_29.text);
				this.e301.e30130 =StringUtil.trim(this.e301_30.text);
				this.e301.e30131 =this.ve301.e30131;
				this.e301.e30133 =this.ve301.e30133;
				this.e301.e30132 = this.ve301.e30132;
				this.e301.e30134 =StringUtil.trim(this.e301_34.text);
				Alert.yesLabel = '是'; 
				Alert.noLabel = '否';
				Alert.show('确认修改？','修改登记信息',1|2,this,updateConfirm);
				//this.e301Service.updateE301(e301);
			}
			
			protected function updateConfirm(event:CloseEvent):void
			{
				if(event.detail == Alert.YES){
					this.e301Service.updateE301(e301);
				}
			}
			
			public function updateE301Handler(event:ResultEvent):void{
				var f301Response:Object =event.result;
				if(!f301Response.state){
					Alert.show(f301Response.errorMessage,"系统提示");
					return;
				}
				Alert.show("修改成功！ ","系统提示");
				dispatchEvent(new Event("ok_click"));
				this.removeWindow();
			}
			private function removeWindow():void
			{ 
				var a:index = this.owner as index;
				a.refresh();
				PopUpManager.removePopUp(this);
			}
		]]>
	</fx:Script>
