			//读卡(母亲)
			protected function readCard_mother(event:MouseEvent):void
			{
				try
				{
					
					var arr:Array = CompMethod.read_card("女");//性别
					if(!arr)
						return ;
					this.D601_05.text = arr[0];//姓名
					this.D601_65.text = arr[5];//身份证号
