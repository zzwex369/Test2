					this.D601_67.text = arr[4];//户口地址
					setD601_07(null);
					var nation:String = arr[2] + "族";//民族
					var nationDataProvider:ArrayCollection = ArrayCollection(this.D601_09.dataProvider);
					var nationIndex:int = getNationDropDownListSelectedIndex(nationDataProvider,nation);
					this.D601_09.selectedIndex = nationIndex;	
					
