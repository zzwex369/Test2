			public function validate1():int
			{
				if(this.D601_05.text == "")
				{
					Alert.show("请输入孕产妇的姓名","系统提示");
					return 1;
				}
				if(this.D601_86.selectedItem.data == 0){
					Alert.show("请选择产妇证件类别","系统提示");
					return 1;
				}
				if(this.D601_65.text == "")
				{
					Alert.show("请输入孕产妇的证件号码","系统提示");
					return 1;
				}
				var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
				if(this.D601_86.selectedItem.data == 1 && this.D601_85.selectedItem.data == "CHN" && !patternMonther.test(this.D601_65.text))
				{	//只有在证件类别为身份证，国籍为中国的时候才验证身份证格式是否正确
					Alert.show("孕产妇身份证号码有误!","系统提示");
					return 1;
				}
				if(this.D601_07.text == "")
				{
					Alert.show("请输入孕产妇年龄!","系统提示");
					return 1;
				}
				if(this.D601_09.selectedItem.data == 0)
				{
					Alert.show("请选择孕产妇民族!","系统提示");
					return 1;
				}
				if(this.D601_67.text == "")
				{
					Alert.show("请输入孕产妇户口地址!","系统提示");
					return 1;
				}
				if(this.D601_03.text == "")
				{
					Alert.show("请选择分娩日期!","系统提示");
					return 1;
				}
				if(this.D601_04.text == "")
				{
					Alert.show("请输入孕产妇住院号!","系统提示");
					return 1;
				}
				if(this.D601_12.text == "")
				{
					Alert.show("请选择末次月经日期!","系统提示");
					return 1;
				}
				var fenmian:Number = D601_03.selectedDate.time; //分娩日期
				var yuejing:Number = D601_12.selectedDate.time; //末次月经
				var jianka:Number = D601_13.selectedDate.time; //建卡日期
				
				if(fenmian<yuejing){
					Alert.show("分娩日期不能小于末次月经日期","系统提示");
