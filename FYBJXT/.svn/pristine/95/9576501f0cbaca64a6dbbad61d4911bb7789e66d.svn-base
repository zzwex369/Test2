					if(StringUtil.trim(this.D601_21.text).length > 2){
						Alert.show("孕周不正确!","系统提示");
						return 1;
					}
				}
				if(this.D601_22.text != "0")
				{
					if(Number(StringUtil.trim(this.D601_22.text)) > 6){
						Alert.show("孕周天数不能大于7!","系统提示");
