			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				
				if(flag == "S601_01")
				{
					this.D601_06.dataProvider = comboBoxDataList;
					this.D601_06.selectedIndex = 3;
				}else if(flag == "S601_02")
				{
					this.D601_08.dataProvider = comboBoxDataList;
					this.D601_08.selectedIndex = 1;
				}else if(flag == "S301_04")
				{
					this.D601_09.dataProvider = comboBoxDataList;
					this.D601_09.selectedIndex = 1;
					this.D601_74.dataProvider = comboBoxDataList;
					this.D601_74.selectedIndex = 1;
				}else if(flag == "D101")
				{
					var index:int = this.getDropDownListSelectedIndex(comboBoxDataList, this.systemUser.institutionCode);
					this.D601_02.dataProvider = comboBoxDataList;
					this.D601_57.dataProvider = comboBoxDataList;
					this.D601_02.selectedIndex = index;
					this.D601_57.selectedIndex = index;
