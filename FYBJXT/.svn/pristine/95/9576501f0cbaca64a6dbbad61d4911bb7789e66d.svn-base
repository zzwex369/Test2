					Alert.show("请输入孕次!","系统提示");
					return 1;
				}
				if(this.D601_20.text == "")
				{
					Alert.show("请输入产次!","系统提示");
					return 1;
				}
				if(this.D601_14.text != "")
				{
					if(StringUtil.trim(this.D601_14.text).length > 2){
						Alert.show("建卡孕周不正确!","系统提示");
						return 1;
					}
				}
				if(this.D601_15.text != "0")
				{
					if(Number(StringUtil.trim(this.D601_15.text)) > 6){
						Alert.show("建卡孕周天数不能大于7!","系统提示");
						return 1;
					}
				}
				var min1:Number = Number(this.D601_29b.text);
				if(min1<0 || min1>=60){
					Alert.show("第一产程分钟有误：请输入0-60之间的数字!","系统提示");
					return 1;
				}
				var min2:Number = Number(this.D601_30b.text);
				if(min2<0 || min2>=60){
					Alert.show("第二产程分钟有误：请输入0-60之间的数字！","系统提示");
