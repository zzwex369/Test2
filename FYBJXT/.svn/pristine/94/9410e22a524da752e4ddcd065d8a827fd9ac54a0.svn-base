			public function saveD604Handler(event:ResultEvent):void{
				var d604Response:Object = event.result;
				if(d604Response.errorMessage == null || d604Response.errorMessage == ""){
					Alert.show(d604Response.promptMessage,"系统提示");
					this.saveButton.enabled = false;
					var url:String = this.parentApplication.contextRoot + "/YcfswServlet?flagF=0&d60406=" + this.d604.d60406 ;
					if(this.d604.d60404!=null && this.d604.d60404!=""){
						url = url+"&d60404=" + encodeURIComponent(this.d604.d60404);
					}
					navigateToURL(new URLRequest(url), null);
				}else {
					Alert.show(d604Response.errorMessage,"系统提示");
				}
			}
