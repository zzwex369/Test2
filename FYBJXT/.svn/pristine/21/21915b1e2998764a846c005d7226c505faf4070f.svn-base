			private function E304_02LabelFunction(item:Object):String
			{
				
				var label:String=item[E304_02.labelField];
				if(item.data)
				{
					label=label.substring(0,label.length-1);
					label=Number(label)/12+'岁';
				}
				return label;
				
			}
			
