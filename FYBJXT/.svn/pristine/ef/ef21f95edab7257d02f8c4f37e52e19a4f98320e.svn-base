					CompMethod.setRadioButtonSelect(L402_97,vl402.l40297);
					CompMethod.setRadioButtonSelect(L402_98,vl402.l40298);

					
					CompMethod.setCheckBoxAutomationName(L402_92,vl402.l40292);
					CompMethod.setCheckBoxAutomationName(L402_99,vl402.l40299);

					if(vl402.l40289)
					{
						var splitArr:Array=vl402.l40289.split(",");
						L402_89_01.text=splitArr[0];
						L402_89_02.text=splitArr[1];
					}
					
						
				
					L402_93.text=vl402.l40293;
					
					if(vl402.l40296)
					{
						splitArr=vl402.l40296.split(",");
						L402_96_01.text=splitArr[0];
						L402_96_02.text=splitArr[1];
					}
					
