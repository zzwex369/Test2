						splitArr=vl402.l402181.split(",");
						L402_181_01.text=splitArr[0];
						L402_181_02.text=splitArr[1];
					}
					
					if(vl402.l402183)
					{
						splitArr=vl402.l402183.split(",");
						L402_183_01.text=splitArr[0];
						L402_183_02.text=splitArr[1];
					}
					
					if(vl402.l402185)
					{
						splitArr=vl402.l402185.split(",");
						L402_185_01.text=splitArr[0];
						L402_185_02.text=splitArr[1];
					}
					
					if(vl402.l402187)
