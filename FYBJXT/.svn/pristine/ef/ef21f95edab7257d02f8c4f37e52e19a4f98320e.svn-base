			/*******************乳腺X线检查*****************/
			private function _saverxxx(l402:L402,arr:Array):void
			{
				this.mouseChildren=false;	
				l402.l40202=l40202;
			    var l402Request:L402Request=new L402Request();
				
				if(l402.l402162=='1')
				{
					l402.l40287=CompMethod.getRadioButtonValue(L402_87);
					l402.l40288=CompMethod.getRadioButtonValue(L402_88);
					l402.l40290=CompMethod.getRadioButtonValue(L402_90);
					l402.l40291=CompMethod.getRadioButtonValue(L402_91);
					l402.l40292=arr[0];

					l402.l40289=StringUtil.trim(L402_89_01.text)+","+StringUtil.trim(L402_89_02.text);
					
					
					l402.l40293=StringUtil.trim(L402_93.text);
					l402.l40294=CompMethod.getRadioButtonValue(L402_94);
					l402.l40295=CompMethod.getRadioButtonValue(L402_95);
					l402.l40297=CompMethod.getRadioButtonValue(L402_97);
					l402.l40298=CompMethod.getRadioButtonValue(L402_98);
					l402.l40299=arr[1];

					
				
					l402.l40296=StringUtil.trim(L402_96_01.text)+","+StringUtil.trim(L402_96_02.text);
					l402.l402181=StringUtil.trim(L402_181_01.text)+","+StringUtil.trim(L402_181_02.text);
					l402.l402183=StringUtil.trim(L402_183_01.text)+","+StringUtil.trim(L402_183_02.text);
					l402.l402185=StringUtil.trim(L402_185_01.text)+","+StringUtil.trim(L402_185_02.text);
					l402.l402187=StringUtil.trim(L402_187_01.text)+","+StringUtil.trim(L402_187_02.text);
					l402.l402182=arr[2];
					l402.l402184=arr[3];
					l402.l402186=arr[4];
					l402.l402188=arr[5];
					
				
					l402.l402100=StringUtil.trim(L402_100.text);
					
					l402.l402102=L402_102.selectedItem.data;
					l402.l402103=StringUtil.trim(L402_103.text);
					l402.l402104=DateField.stringToDate(L402_104.text,'YYYY-MM-DD');
					
					
				}
				l402Request.picList=picData;
				l402.l402164=StringUtil.trim(L402_164.text);
				
				
				l402.l402128=L402_128.selectedItem.data;
				l402.l402129=L402_128.selectedItem.data+'01';
				l402.l402133=L402_128.selectedItem.label;
				l402.l402130=StringUtil.trim(L402_130.text);
				l402.l402131=DateField.stringToDate(L402_131.text,'YYYY-MM-DD');
				l402.l402132=l402.l402131;
				l402Request.flag="3";
				if("0"==state)
