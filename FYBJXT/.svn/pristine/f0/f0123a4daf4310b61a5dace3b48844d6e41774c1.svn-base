			private function setMontherBirthday(event:Event):void
			{
				if(this.F303_06.selectedItem.data == 1){
					var cardId:String = this.F303_07.text;
					if(cardId.length>=16){
						cardId=cardId.substring(6,14);
						this.F303_08.text = cardId.substring(0,4) + "-" + cardId.substring(4,6) + "-" + cardId.substring(6,8);
					}else{
						cardId=cardId.substring(6,12);
						this.F303_08.text = "19" + cardId.substring(0,2) + "-" + cardId.substring(2,4) + "-" + cardId.substring(4,6);
					}
				}
			}
			//验证信息
			public function validate1():void
			{
				if(StringUtil.trim(this.F303_05.text) ==""){
					Alert.show("请输入孕产妇的姓名","系统提示");
					return;
				}
				if(this.F303_06.selectedItem.data ==0){
					Alert.show("请选择孕产妇的证件类型","系统提示");
					return;
				}
				if(StringUtil.trim(this.F303_07.text) ==""){
					Alert.show("请输入孕产妇的证件号码","系统提示");
					return;
				}
				if(this.F303_08.text ==""){
					Alert.show("请输入孕产妇的出生日期","系统提示");
					return;
				}
				if(this.F303_124.selectedItem.data ==0){
					Alert.show("请选择孕产妇的国籍","系统提示");
					return ;
				}
				var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
				if(this.F303_06.selectedItem.data==1 && this.F303_124.selectedItem.data=="CHN") 
				{
					if(!patternMonther.test(this.F303_07.text)){
						Alert.show("母亲身份证号码有误!","系统提示");
						return;
					}
				}
			    if(StringUtil.trim(this.F303_04.text) ==null){
					Alert.show("请输入建册编号!","系统提示");
					return;
				} 
				if(this.F303_17.text ==""){
					Alert.show("请输入检查医生!","系统提示");
					return;
				}
				if(this.F303_15.selectedItem.data ==0){
					Alert.show("请选择检查医院!","系统提示");
					return;
				}
				if(this.F303_14.text == ""){
					Alert.show("请选择检查日期!","系统提示");
					return;
				}
				if(this.F303_123.selectedItem.data ==0){
					Alert.show("请选择检查阶段!","系统提示");
					return;
				}
				this.savePerinata();
			}
			public function setF303_08(event:Event):void{
				if(this.F303_06.selectedItem.data == 1){
					var cardId:String  = this.F303_07.text;
					if(cardId.length >= 16){
						var year:Number = Number(this.F303_07.text.substr(6,4));
						var month:Number = Number(this.F303_07.text.substr(10,2));
						var day:Number = Number(this.F303_07.text.substr(12,2));
						var date:Date = new Date();
						var nowDay:Number = date.date;
						var nowMonth:Number = date.month + 1;
						var nowYear:Number = date.fullYear;
						
						this.F303_08.text =year+"-"+month+"-"+day;
					}
				}
			}
