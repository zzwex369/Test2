			public function queryF303(vf302:VF302):void
			{
				f303Request = new F303Request();
				f303Request.name =vf302.f30203;
				f303Request.idcard =vf302.f30205;
				f303Request.f303123_flag ="孕后";
				f303Request.parameterPageindex = 1;
				f303Request.parameterPagesize = 3;
				this.maternalCheckService.queryF303(f303Request);
				this.certificateDataGrid.addEventListener(GridSelectionEvent.SELECTION_CHANGE,onSelected);
			}
			private function onSelected(event:GridSelectionEvent):void
			{
				this.vf303 = DataGrid(event.target).selectedItem as VF303;
			}
			protected function queryF303Handler(event:ResultEvent):void
			{
				var f303Response:Object = event.result;
				var flag:String = f303Response.flag;
				var listVF303:ArrayCollection = f303Response.vf303s;
				var rowCount:Number = f303Response.rowCount;
				if(f303Response.errorMessage == null)
				{ 
					this.pagerBar.pageSize = 2;
					this.pagerBar.dataGrid = this.certificateDataGrid;
					this.pagerBar.rowCount = rowCount;
					this.pagerBar.resultData = listVF303;
					this.pagerBar.dataBind();
				} else {
					Alert.show(f303Response.errorMessage, "系统提示");
				}
			}
			private function query(event:MouseEvent):void{
				if(this.vf303 == null){
					Alert.show("请选择要操作的行！","系统提示");
					return;
				}
				var window:HistoryWindow = new HistoryWindow();
				window.owner = this;
				PopUpManager.addPopUp(window,this);
				var x:Number = (this.parentApplication.mdiCanvas.width - window.width)/2;
				var y:Number = (this.parentApplication.mdiCanvas.height - window.height)/2;
				window.move(x,y);
				window.vf303 = this.vf303;
			}
			public function validateGaowei():void
			{
				var aa1:int=0;
				var aa2:int=0;
				var aa4:int=0;
				var aa5:int=0;
				var aa12:int=0;
				var aa13:int=0;
				var aa14:int=0;
				/* if(this.F303_41.value ==0){ //身高
				aa1=0;
				}else if(this.F303_41.value >0 &&this.F303_41.value <145){ 
				aa1=10;
				}else if(this.F303_41.value ==145){   
				aa1=10;
				}else{
				aa1 =0;
				} */	
				if(this.F303_42.value ==0){  
					aa2=0;
				}else if(this.F303_42.value >0 && this.F303_42.value <80){  //体重
					aa2=5;
				}else if(this.F303_42.value ==80){ 
					aa2=5;
				}else if(this.F303_42.value >80 && this.F303_42.value <160){ 
					aa2=5;
				}else{
					aa2=0;
				}  
				if(this.F303_75.value ==0){  //血红蛋白 
					aa4 =0;
				}else if(this.F303_75.value >0 && this.F303_75.value <60){
					aa4 =10;
				}else if(this.F303_75.value ==60){  
					aa4 =5;
				}else if(this.F303_75.value >60 && this.F303_75.value <100){  
					aa4 =5;
				}else{
					aa4 =0;
				}  
				if(this.F303_87.selectedItem.data ==2){ //血糖 异常
					aa5 =15;
				}else{
					aa5 =0;
				} 
				//高血压
				if(this.F303_72.value ==0){  //骶耻外径
					aa12 =0;
				}else if(this.F303_72.value >0 &&this.F303_72.value <18){  
					aa12 =10;
				}else{
					aa12 =0;
				}
				if(this.F303_73.value ==0){ //坐骨结节间径
					aa13 =0;
				}else if(this.F303_73.value >0 &&this.F303_73.value <8){ 
					aa13 =10;
				}else if(this.F303_73.value ==8){ 
					aa13 =10;
				}else{
					aa13 =0;
				}
				if(this.F303_55.value ==0){  //胎心率  次/分
					aa14 =0;
				}else if(this.F303_55.value >0 && this.F303_55.value <100){
					aa14 =15;
				}else if(this.F303_55.value ==100){  
					aa14 =15;
				}else if(this.F303_55.value <120 && this.F303_55.value >100){  
					aa14 =10;
				}else if(this.F303_55.value ==120){  
					aa14 =10;
				}else if(this.F303_55.value ==160){  
					aa14 =10;
				}else if(this.F303_55.value >160){  
					aa14 =10;
				}else{
					aa14 =0;
				}
				
				this.F303_11.text =(aa1+aa2+aa4+aa5+aa12+aa13+aa14+f30255_).toString();
				if(Number(this.F303_11.text) ==5){
					this.F303_12.selectedIndex =2;
				}else if(Number(this.F303_11.text) ==10){
					this.F303_12.selectedIndex =3;
				}else if(Number(this.F303_11.text) >10 && Number(this.F303_11.text) <15){
					this.F303_12.selectedIndex =3;
				}else if(Number(this.F303_11.text) ==15){	
					this.F303_12.selectedIndex =3;
				}else if(Number(this.F303_11.text) ==20){
					this.F303_12.selectedIndex =4;
				}else if(Number(this.F303_11.text) >20){
					this.F303_12.selectedIndex =4;
				}else{
					this.F303_12.selectedIndex =1;
				} 
				this.F303_13.text ="围产保健高危评分值:"+f30255_
					+";身高高危评分:"+aa1+";体重评分:"+aa2+";血红蛋白评分:"+aa4
					+";胎心率:"+aa14+";骶耻外径:"+aa12+";坐骨结节间径:"+aa13+";血糖评分:"+aa5;
			}
		]]>
