			private function clearSelected(id:Object,textId:Object):void
			{
				for(var j:int=0;j<id.numElements;j++)
				{
					var checkBox:Object= id.getElementAt(j);
					if(checkBox.selected)
						checkBox.selected=false;
					textId.text="无";
				}
			}
			
