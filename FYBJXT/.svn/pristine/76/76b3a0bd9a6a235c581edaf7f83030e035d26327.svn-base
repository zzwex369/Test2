				}
				if(!SW303_07.selectedIndex)
				{
					Alert.show("请选择母亲民族!","系统提示");
					return;
				}
				if(SW303_04.selectedIndex==1)
				{
					var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
					if(!patternMonther.test(StringUtil.trim(SW303_05.text)))
					{
						Alert.show("母亲身份证号码有误!","系统提示");
						return;
					}
				}
				if(!SW303_06.text)
				{
					Alert.show("请输入母亲年龄","系统提示");
					return;
				}
