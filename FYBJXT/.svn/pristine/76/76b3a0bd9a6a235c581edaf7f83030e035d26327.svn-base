			/**
			 *
			 */
			private function setFieldVal(radioValue:String,textVal:String,suffixField:Array):void
			{
				var radioVal:Number=Number(radioValue);
				textVal=StringUtil.trim(textVal);
				var compareVal:int=6;
				for(var i in suffixField)
				{
					sw303[suffixField[i]]=radioVal==compareVal?textVal:'';
					compareVal++;
