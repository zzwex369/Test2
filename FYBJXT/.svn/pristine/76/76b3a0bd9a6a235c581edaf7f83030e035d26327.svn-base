			private function renewSw303Handler(event:ResultEvent):void
			{
				var sw303Response:Object =  event.result;
				if(!sw303Response.state) {
					saveSW303.enabled=true;
					Alert.show(sw303Response.errorMessage,"系统提示");
					return;
				}
				var thisObj:Object=this;
				Alert.show("修改成功！ ","系统提示",1,this,
					function ():void
					{
						removeWindow(true);
					}
				);
			}
			
			
