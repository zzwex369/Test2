			}
			
			/**
			 *
			 */
			private function clearText(idEles:*):void
			{
				if(idEles is Array)
				{
					for(var i:String in idEles)
					{
						doClearText(idEles[i]);
					}
				}else
					doClearText(idEles);
			}
			
			/**
			 *
			 */
			private function doClearText(id:Object):void
			{
				id.text='';
			}
			
			private function zdyj_changeHandler(explain:Object,text:String='',
												enabled:Boolean=false,label:Object=null):void
			{
				
				if(!enabled)
					explain.text='';
				else
					label.text=text;
			}
			/**
