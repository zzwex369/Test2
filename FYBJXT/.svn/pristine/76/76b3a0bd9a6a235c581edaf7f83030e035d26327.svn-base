			private function getComboBoxData(sql:String,flag:String,showPrompt:Boolean=false):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = showPrompt;
				comboBoxDataRequest.sql =sql ;
				comboBoxDataRequest.flag = flag;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			public function creationCompleteHandler(vSw303:Object):void
			{
				this.vSw303=vSw303;
				this.systemUser = this.parentApplication.systemUser;
				getComboBoxData("select t.d101_01, t.d101_02 from D101 t where t.D101_01 = " +
					this.systemUser.institutionCode +" or t.D101_01 = "+vSw303.sw30384
					,"D101",false);
				
				getComboBoxData("select s_01,s_02 from S301_01 ","S301_01");
				
				getComboBoxData("select s_01,s_02 from S301_04 ","S301_04",true);
				
				getComboBoxData("select s_01,s_02 from S301_06 ","S301_06",true);
				
				
			}
			
			/**
			 *
			 */
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				if(flag == "D101")
				{
					this.SW303_84.dataProvider = comboBoxDataList;
					this.SW303_84.selectedIndex = 0;	
				}else if(flag=="S301_01")
				{
					SW303_21.dataProvider=comboBoxDataList;
					SW303_21.selectedIndex=0;
				}else if(flag=="S301_04")
				{
					SW303_07.dataProvider=comboBoxDataList;
					SW303_18.dataProvider=comboBoxDataList;
					SW303_07.selectedIndex=1;
					SW303_18.selectedIndex=0;
					
				}else if(flag=="S301_06")
				{
					SW303_04.dataProvider=comboBoxDataList;
					SW303_15.dataProvider=comboBoxDataList;
					SW303_04.selectedIndex=1;
					SW303_15.selectedIndex=0;
					
				}
				--count;
				if(!count)
					putSw303();
			}
			
			/**
			 *
			 */
			private function hasVal(valArr:Array):String
			{
				for each(var val in valArr)
				{
					if(val)
						return val;
				}	
				return '';
			}
			
