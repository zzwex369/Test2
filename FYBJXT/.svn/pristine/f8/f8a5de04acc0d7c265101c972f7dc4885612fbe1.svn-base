			private var comboBoxDataRequest:ComboBoxDataRequest;
			private var systemUser:SystemUser;
			private var l302:L302;
			public  var l30202:String;
			public var l30204:String;
			public var state:String;
			public var vl302:VL302;
			private var i:int=0;
			private var fkCheckBoxResult:Array;
			private var fkqtCheckBoxResult:Array;
			private function init():void
			{
				initializeComboBox();
				systemUser=this.parentApplication.systemUser;
			}
			
			private function _initializeComboBox(sql:String,flag:String,showPrompt:Boolean=true):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.sql = sql;
				comboBoxDataRequest.flag = flag;
				comboBoxDataRequest.showPrompt = showPrompt;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			/******************下拉框填充数据******************/
			private function initializeComboBox():void
			{
				
				_initializeComboBox("select * from LA302_30 t","LA302_30");
				
				
			}
			/******************得到数据******************/
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
			   if(flag == "LA302_30")
				{
					L302_44_02.dataProvider=comboBoxDataList;
					L302_44_02.selectedIndex=1;
				}
				if("0"==state)
				{
					CompMethod.hasNumEles(vg);				
					callLater(updatefkjc);
				}
			}
			
			private function _savefk(l30241:String):void
			{
			
				
				save.enabled=false;
				var l302Request:L302Request=new L302Request();
				l302Request.flag="1";
				l302.l30235=StringUtil.trim(L302_35.text);
				l302.l30237=StringUtil.trim(L302_37.text);
				l302.l30239=StringUtil.trim(L302_39.text);
				l302.l30255=StringUtil.trim(L302_55.text);
				l302.l30241=l30241;

				l302.l30243=StringUtil.trim(L302_43_02.text);
				l302.l30244=L302_44_02.selectedItem.data;
				l302.l30245=StringUtil.trim(L302_45_01.text)+","+
					StringUtil.trim(L302_45_02.text)+","+
					StringUtil.trim(L302_45_03.text);
				
				l302.l30246=StringUtil.trim(L302_46.text);	
				
				l302.l30250=StringUtil.trim(L302_50_01.text)+","+
					StringUtil.trim(L302_50_02.text)+","+
					StringUtil.trim(L302_50_03.text);
				
				l302.l30249=StringUtil.trim(L302_49.text);	
				l302.l30252=StringUtil.trim(L302_52.text);
				l302.l30258=StringUtil.trim(L302_58.text);

				l302.l30259=checkfk.institutionCode.selectedItem.data;
				l302.l30260=StringUtil.trim(checkfk.checker.text);
				l302.l30261=DateField.stringToDate(checkfk.nowDate.text,"YYYY-MM-DD");
				
				l302.l30254=CompMethod.getRadioButtonValue([L302_54]);
				l302.l30256=CompMethod.getRadioButtonValue([L302_56]);	
			
			
				
				if("0"==state)
				{
					l302.l30201=vl302.l30201;
					l302Request.l302=l302;
					l302Service.renewL302(l302Request);
				}else
				{			
					l302.l30202=l30202;
					l302Request.l302=l302;
					l302Service.saveL302(l302Request);
				}
			}
			private function saveOrRenewL302Handler(event:ResultEvent):void
			{
				var l302Response:Object=event.result;
				save.enabled=true;
				if(l302Response.state)
				{
					Alert.show("0"==state?"修改成功":"保存成功！","系统提示",1,this,closeHandler);
					return ;
				}
				Alert.show(l302Response.errorMessage,"系统提示");
			}
			
			/**
			 *
			 */
			private function closeHandler(event:CloseEvent):void
			{
				if(event.detail==1)
					this.dispatchEvent(new Event("saveSuccess"));
			}
			
			
			private function updatefkjc():void
			{
				L302_35.text=vl302.l30235;
				L302_37.text=vl302.l30237;
				L302_39.text=vl302.l30239;

				L302_43_02.text=vl302.l30243;
				L302_46.text=vl302.l30246;
				L302_49.text=vl302.l30249;
				L302_52.text=vl302.l30252;
				L302_55.text=vl302.l30255;
				L302_58.text=vl302.l30258;

				checkfk.checker.text=vl302.l30260;

				if(vl302.l30245)
				{
					var l30245:Array=vl302.l30245.split(",");
					L302_45_01.text=l30245[0];
					L302_45_02.text=l30245[1];
					L302_45_03.text=l30245[2];
				}
				
				
				
				if(vl302.l30250)
				{
					var l30250:Array=vl302.l30250.split(",");
					L302_50_01.text=l30250[0];
					L302_50_02.text=l30250[1];
					L302_50_03.text=l30250[2];
				}
				
				
				
				CompMethod.setCheckBoxAutomationName([L302_34],vl302.l30234);
				CompMethod.setCheckBoxAutomationName([L302_38],vl302.l30238);
				CompMethod.setCheckBoxAutomationName([L302_40],vl302.l30240);
				CompMethod.setCheckBoxAutomationName([L302_41],vl302.l30241);

				CompMethod.setCheckBoxAutomationName([L302_42],vl302.l30242);
				CompMethod.setCheckBoxAutomationName([L302_47],vl302.l30247);
				CompMethod.setCheckBoxAutomationName([L302_53],vl302.l30253);
						
				CompMethod.setRadioButtonSelect([L302_48],vl302.l30248);
				CompMethod.setRadioButtonSelect([L302_51],vl302.l30251);
				CompMethod.setRadioButtonSelect([L302_54],vl302.l30254);
				CompMethod.setRadioButtonSelect([L302_56],vl302.l30256);
				CompMethod.setRadioButtonSelect([L302_166],vl302.l302166);
				
				CompMethod.setCheckBoxAutomationName([L302_36_1],vl302.l30236);
				
				CompMethod.setCheckBoxAutomationName([L302_57_1,L302_57_2,L302_57_3],vl302.l30257);

				checkfk.nowDate.text=vl302.l30261str;
				L302_44_02.selectedIndex=CompMethod.getDropDownListSelectedIndex(L302_44_02.dataProvider,vl302.l30244);
			
				callLater(institutionCodeDataProvider);
			}
			
			
			private function institutionCodeDataProvider():void
			{
				if(!(checkfk.institutionCode.dataProvider))
				{
					callLater(institutionCodeDataProvider);
					return ;				
				}
				checkfk.institutionCode.selectedIndex=CompMethod.getDropDownListSelectedIndex(checkfk.institutionCode.dataProvider,vl302.l30259);
			}
			
			
			/**
			 *
			 */
			private function getFkSelectVal():void
			{
				fkCheckBoxResult=new Array(7);//是否一项未选
				fkqtCheckBoxResult=new Array(2);//其他 和异常 多选 数组
				l302.l30248=CompMethod.getRadioButtonValue([L302_48]);
				l302.l30251=CompMethod.getRadioButtonValue([L302_51]);			
				l302.l302166=CompMethod.getRadioButtonValue([L302_166]);
				
				
				fkCheckBoxResult[0]=CompMethod.getCheckBoxAutomationName([L302_34]);
				fkCheckBoxResult[1]=CompMethod.getCheckBoxAutomationName([L302_36_1]);
				fkCheckBoxResult[2]=CompMethod.getCheckBoxAutomationName([L302_38]);
				fkCheckBoxResult[3]=CompMethod.getCheckBoxAutomationName([L302_40]);
				fkCheckBoxResult[4]=CompMethod.getCheckBoxAutomationName([L302_42]);
				fkCheckBoxResult[5]=CompMethod.getCheckBoxAutomationName([L302_47]);
				fkCheckBoxResult[6]=CompMethod.getCheckBoxAutomationName([L302_53]);
				
				
				l302.l30234=fkCheckBoxResult[0][1];
				l302.l30236=fkCheckBoxResult[1][1];
				l302.l30238=fkCheckBoxResult[2][1];
				l302.l30240=fkCheckBoxResult[3][1];
				l302.l30242=fkCheckBoxResult[4][1];
				l302.l30247=fkCheckBoxResult[5][1];
				l302.l30253=fkCheckBoxResult[6][1];
				
				fkqtCheckBoxResult[0]=CompMethod.getCheckBoxAutomationName([L302_57_1,L302_57_2,L302_57_3]);
				
				l302.l30257=fkqtCheckBoxResult[0][1];
				
				
			}
			private function savefkjc():void
			{
				l302=new L302();
				getFkSelectVal();
				
				if(!fkCheckBoxResult[0][0])
				{
					Alert.show("请选择外阴检查","系统提示");
					return ;
				}
				
				if(!fkCheckBoxResult[1][0])
				{
					Alert.show("请选择外阴检查 ","系统提示");
					return ;
				}
				
				if(!fkCheckBoxResult[2][0])
				{
					Alert.show("请选择分泌物","系统提示");
					return ;
				}
				
				if(!fkCheckBoxResult[3][0])
				{
					Alert.show("请选择子宫颈检查 ","系统提示");
					return ;
				}
				
				if(!fkCheckBoxResult[4][0])
				{
					Alert.show("请选择子宫检查","系统提示");
					return ;
				}
				
				if(!fkCheckBoxResult[5][0])
				{
					Alert.show("请选择附件（盆腔）","系统提示");
					return ;
				}
				
				if(L302_35.enabled&&!L302_35.text)
				{
					Alert.show("请输入外阴检查其他","系统提示");
					return ;
				}
				
				if(L302_37.enabled&&!L302_37.text)
				{
					Alert.show("请输入阴道检查其他","系统提示");
					return ;
				}
				
				if(L302_39.enabled&&!L302_39.text)
				{
					Alert.show("请输入分泌物其他 ","系统提示");
					return ;
				}
				
				if(L302_49.enabled&&!L302_49.text)
				{
					Alert.show("请输入赘生物大小","系统提示");
					return ;
				}
				
				if(L302_55.enabled&&!L302_55.text)
				{
					Alert.show("请输入分泌物检查其他","系统提示");
					return ;
				}
				
				if(L302_46.enabled&&!L302_46.text)
				{
					Alert.show("请输入子宫其他","系统提示");
					return ;
				}
				
				if(L302_52.enabled&&!L302_52.text)
				{
					Alert.show("请输入附件其他","系统提示");
					return ;
				}
				
				if(L302_166.enabled&&!l302.l302166)
				{
					Alert.show("请选择子宫脱垂程度","系统提示");
					return ;
				}
				
				if(L302_48.enabled&&!l302.l30248)
				{
					Alert.show("请选择附件压痛位置","系统提示");
					return ;
				}
				
				if(L302_51.enabled&&!l302.l30251)
				{
					Alert.show("请选择附件肿物位置","系统提示");
					return ;
				}
				
				var L30241Arr:Array=CompMethod.getCheckBoxAutomationName(L302_41);
				if(L302_40_09.selected&&!L30241Arr[0])
				{
					Alert.show("请选择子宫颈检查其他","系统提示");
					return ;
				}
				
				if(L302_56_02.selected&&!fkqtCheckBoxResult[0][0])
				{
					Alert.show("请选择异常情况","系统提示");
					return ;
				}
				
				if(L302_58.enabled&&!L302_58.text)
				{
					Alert.show("请输入妇科检查临床诊断 异常 其他 ","系统提示");
					return ;
				}
				if(!checkfk.nowDate.text)
				{
					Alert.show("检查时间不能为空","系统提示");
					return ;
				}
				
				if(!checkfk.checker.text)
				{
					Alert.show("请输入检查人员信息","系统提示");
					return;
				}
				_savefk(L30241Arr[1]);
			}
			
			private function changeHandler(event:Event,ids:*):void
			{
				var hgroup:Object=ids is Array?ids[0]:ids;
				var firstEle:Object=hgroup.getElementAt(0);
				var id:String=event.target.id;
				var select:Boolean=event.target.selected;
				if(firstEle==event.target)
				{
					if(firstEle.selected)
					{
						CompMethod.hasNumEles(ids);
						
						callLater(function ():void
						{
							firstEle.selected=!firstEle.selected;
						});
					}
					
				}else
				{
					if(firstEle.selected)
						firstEle.selected=!firstEle.selected;
					getAutomationNameEle(id,ids,select?false:true);
				}	
				
				
			}
			
			/**
			 *清空设置值
			 */
			private function getAutomationNameEle(id:String,ids:*,isClear:Boolean):void
			{
				if(ids is Array)
				{
					for(var i:int=0;i<ids.length;i++)
					{
						doGetAutomationNameEle(id,ids[i],isClear);
					}
				}else
					doGetAutomationNameEle(id,ids,isClear);
			}
			
			
			/**
			 *
			 */
			private function doGetAutomationNameEle(id:String,ids:*,isClear:Boolean,hasEle:Boolean=true):void
			{
				if(hasEle)
					clearOrSetVal(id,ids,isClear);
				else{
					for(var j:int=0;j<ids.numElements;j++)
					{
						var child:Object=ids.getElementAt(j);
						if(child is TextInput||child is HGroup)
							clearOrSetVal(id,child,isClear);	
					}
				}
				
			}
			
			/**
			 *
			 */
			private function clearOrSetVal(id:String,child:Object,isClear:Boolean=true):void
			{
				var isHgroup:Boolean=child is HGroup;
				var name:String=child.automationName;
				if(name&&name==id)
					isClear?CompMethod.hasNumEles(child):isHgroup?CompMethod.setRadioButtonSelect([child],'1'):null;
				else if(isHgroup)
					doGetAutomationNameEle(id,child,isClear,false);
				
			}
		
			
		]]>
	</fx:Script>
