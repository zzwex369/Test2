			private function slsc_resizeHandler(event:Event):void
			{
				if(!oldTgTab)
					oldTgTab=tgTab.height;
				var max:int=slsc.height>tgNav.height?slsc.height:tgNav.height;
				max+=34;
				_diff=max-oldTgTab;
				slNav.height+=_diff;
				tgTab.height=max;
				oldTgTab=max;
				if(ve304)
					this.height+=_diff;
				dispatchEvent(new Event("resizejcdj"));
			}
			
