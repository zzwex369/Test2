			private function E304_02_changeHandler(event:IndexChangeEvent):void
			{
				slsc.slChange(E304_02.selectedItem.data);//儿童视力根据月龄变化
				CompMethod._walk(E304_52);
				var newIdx:int=event.newIndex;
				if(newIdx>0)
					setRange();
				E304_18.selectedIndex=1;
				E304_12.selectedIndex=2;
				E304_46.selectedIndex=3;
				if(newIdx==1)
				{
					
					E304_27.text='';//血红蛋白值g/L
					E304_46.selectedIndex=0;//步态 
					CompMethod.setCheckBoxAutomationName(E304_52,'1,2,3,6');
				}
				else if(newIdx==2)
				{
					E304_18.selectedIndex=0;//听力
					CompMethod.setCheckBoxAutomationName(E304_52,'1,2,4,6');
				}
				else if(newIdx==3)
				{
					
					E304_27.text='';//血红蛋白值g/L
					CompMethod.setCheckBoxAutomationName(E304_52,'1,2,4,6');
				}
				else if(newIdx==4)
