					E304_13.text='';//前卤
					E304_14.text='';//前卤
					E304_29.text='';//服用维生素D
					E304_12.selectedIndex=1;
					E304_18.selectedIndex=0;
					CompMethod._walk(E304_25);
					CompMethod.setCheckBoxAutomationName(E304_52,'1,2,5,6');
				}
			}
			
		
			
			
