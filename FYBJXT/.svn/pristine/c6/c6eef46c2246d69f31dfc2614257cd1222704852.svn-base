			//读卡(母亲)
			protected function readCard_mother(event:MouseEvent):void
			{
				try
				{
					//readCard是调用index.js下的函数，读卡涉及index.jsp和index.js两个文件
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						
						var array:Array = str.split(",");
						var sex:String = array[1];//性别
						if(sex=="女"){
							this.F307_02.text = array[0];//姓名
							this.F307_36.text = array[5];//身份证号
							//							this.d301_13.text = array[3];//出生日期
							
							var nation:String = array[2] + "族";//民族
						}else{
							Alert.show("请读母亲信息！","系统提示");
						}
					}else{
						Alert.show("读卡失败！");
					}
				} 
				catch(error:Error) 
				{
					Alert.show("读卡失败！");
				}
			}
			
			public function validate1():void
			{
				if(StringUtil.trim(this.F307_02.text) == ""){
					Alert.show("请输入孕产妇的姓名!","系统提示");
					return;
				}
				if(StringUtil.trim(this.F307_38.text) == ""){
					Alert.show("请输入保健卡号!","系统提示");
					return;
				}
				if(StringUtil.trim(this.F307_36.text) == ""){
					Alert.show("请输入证件号码!","系统提示");
					return;
				}
				if(this.F307_27.selectedItem.data ==0){
					Alert.show("请选择产后访视!","系统提示");
					return;
				}
				if(StringUtil.trim(this.F307_32.text) == ""){
					Alert.show("请输入随访编号!","系统提示");
					return;
				}
				if(this.F307_37.selectedItem.data ==0){
					Alert.show("请选择随访机构!","系统提示");
					return;
				}
				if(this.F307_25.text ==""){
					Alert.show("请输入随访医生!","系统提示");
					return;
				}
				if(StringUtil.trim(this.F307_03.text) == ""){
					Alert.show("请选择随访日期!","系统提示");
					return;
				}
				if(StringUtil.trim(this.F307_24.text) == ""){
					Alert.show("请选择下次随访日期!","系统提示");
					return;
				}
				if(!validateAreaCode(this.F307_39.getAreaCode())){
					Alert.show("产后休养地要选到县区！","系统提示");
					return;
				}
				this.savePerinata();
			}
			private function validateAreaCode(areaCode:String):Boolean
			{
				if(areaCode == "" || areaCode.length == 0){
					return false;
				} else if(areaCode.length < 6){
					return false;
				} else {
					return true;
				}
			}
