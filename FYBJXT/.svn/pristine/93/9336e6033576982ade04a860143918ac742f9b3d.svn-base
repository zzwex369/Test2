<?xml version="1.0" encoding="utf-8"?>
<s:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx"
		  xmlns:component="com.xyw.sys.custom.component.*"
		  xmlns:etsljc="com.xyw.module.etbj.etsl.etjc.ccjc.componet.*"
		  width="100%" height="100%">
	<fx:Script>
		<![CDATA[
			import com.xyw.sys.custom.component.CompMethod;
		]]>
	</fx:Script>
	<fx:Declarations>
		
	</fx:Declarations>
	<fx:Style>
		@namespace s "library://ns.adobe.com/flex/spark";
		@namespace mx "library://ns.adobe.com/flex/mx";
		mx|ToolTip
		{
			fontSize:15;  
			color:#FF6699; 
		}
		s|Label
		{
			fontSize:13;
		} 
		
		mx|GridItem
		{
			horizontalAlign:left;
			verticalAlign:middle;
		}
		
		s|TextInput
		{
			fontFamily:微软雅黑;
		}
		s|DataGrid
		{
			borderAlpha:"0.3";
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomDataGridSkin");
			alternatingRowColors:"[#FFFFFF,#EEEEEE]";
		}
		
		s|Button
		{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomButtonSkin");
		}
		s|DropDownList
		{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomDropDownListSkin");
			selectionColor:#DDDDDD;
			rollOverColor:#EEEEEE;	
		}
		.must {
			color:red;
			fontSize:20px;
			fontWeight:bold;
			paddingTop:5px;
		}
		s|ComboBox{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomComboBoxSkin");
			
		}
	</fx:Style>
	
	<s:VGroup width="100%" height="100%" chromeColor="#ffffff">
		
		<s:Scroller width="100%" height="100%" horizontalScrollPolicy="auto" verticalScrollPolicy="auto">
			<s:VGroup>
				<etsljc:etjc width="1300" paddingBottom="20" paddingLeft="10" paddingRight="10" paddingTop="10"
							 registrationType="1" parentModule="{this}"/>
			</s:VGroup>
		</s:Scroller>
	</s:VGroup>
</s:Module>
