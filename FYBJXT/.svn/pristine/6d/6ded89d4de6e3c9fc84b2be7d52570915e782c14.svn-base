					var e305:E305=new E305();
					e305.e30501 =this.ve305.e30501;
					var e305Request:E305Request = new E305Request();
					e305Request.e305=e305;
					e305Request.parameterPageindex=1;
					e305Request.parameterPagesize=20;
					this.e305Service.queryE305H(e305Request);
				}else{
					Alert.show(e305Response.errorMessage, "系统提示");
				}
			}
			
