			/**
			 *
			 */
			private function removeWindow(isRefresh:Boolean=false):void
			{
				if(isRefresh)
				{
					var index:Object =this.owner;
					index.refresh();
				}
				PopUpManager.removePopUp(this);
			}
			
			
			private function getComboBoxData(sql:String,flag:String,showPrompt:Boolean=true):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = showPrompt;
				comboBoxDataRequest.sql =sql ;
				comboBoxDataRequest.flag = flag;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			public function creationCompleteHandler(vSw302:Object):void
			{
				this.vSw302=vSw302;
				this.systemUser = this.parentApplication.systemUser;
				getComboBoxData("select t.d101_01, t.d101_02 from D101 t where t.D101_01 = " +
					this.systemUser.institutionCode +" or t.D101_01 = "+vSw302.sw30282
					,"D101",false);
				
				getComboBoxData("select s_01,s_02 from S301_01 ","S301_01");
				
				getComboBoxData("select s_01,s_02 from S301_04 ","S301_04",false);
				
				getComboBoxData("select s_01,s_02 from S301_06 ","S301_06",false);
				
				getComboBoxData("select s_01,s_02 from S601_01" ,"S601_01");
				
				getComboBoxData("select * from SW304_02" ,"SW304_02",false);
				
				getComboBoxData("select * from S601_10" ,"S601_10",false);
				
			}
			
			
			
			/**
			 *
			 */
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
				if(flag == "D101")
				{
					this.SW302_82.dataProvider = comboBoxDataList;
					this.SW302_82.selectedIndex = 0;	
				}else if(flag=="S301_01")
				{
					SW302_20.dataProvider=comboBoxDataList;
					SW302_20.selectedIndex=0;
				}else if(flag=="S301_04")
				{
					SW302_12.dataProvider=comboBoxDataList;
					SW302_12.selectedIndex=0;
				}else if(flag=="S301_06")
				{
					SW302_08.dataProvider=comboBoxDataList;
					SW302_08.selectedIndex=0;
					
				}else if(flag=="S601_01")
				{
					SW302_19.dataProvider=comboBoxDataList;
					SW302_19.selectedIndex=0;
					
				}else if(flag=="SW304_02")
				{
					SW302_18.dataProvider=comboBoxDataList;
					SW302_18.selectedIndex=4;
					
				}else if(flag=="S601_10")
				{
					SW302_24.dataProvider=comboBoxDataList;
					SW302_24.selectedIndex=0;
					
				}
				--count;
				if(!count)
					putSw302();
			}
			
			/**
			 *
			 */
			private function doSW30229Val(gridArr:Array):void
			{
				sw30229Arr=vSw302.sw30229.split(',');
				for(var i:int=0;i<gridArr.length;i++)
				{
					setSW30229Val(gridArr[i]);
				}
			}
			
			/**
			 *
			 */
			private function setSW30229Val(id:Object):void
			{
				for(var i:uint=0;i<id.numElements;i++)
				{	
					var child:Object=id.getElementAt(i);
					if (child is Container && Container(child).getChildren()) 
						setSW30229Val(child); 
					else if (child is CheckBox&&child.id)
					{
						for(var j:int=0;j<sw30229Arr.length;j++)
						{
							if(child.automationName==sw30229Arr[j]||!sw30229Arr[j])
							{
								child.selected=sw30229Arr[j]?true:false;
								sw30229Arr.splice(j,1);
								j--;
								break;
							}					
						}
					}
				}
			}
			
