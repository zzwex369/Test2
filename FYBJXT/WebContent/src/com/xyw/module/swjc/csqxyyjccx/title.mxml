			/**
			 *
			 */
			private function validate():void
			{
				sw302.sw30229='';
				getGridID([grid1,grid2]);
				getRadioButtonVal();
				
				if(!SW302_06.text)
				{
					Alert.show("请输入住院号","系统提示");
					return;
				}
				if(!SW302_07.text)
				{
					Alert.show("请输入母亲姓名","系统提示");
					return;
				}
				if(!SW302_09.text)
				{
					Alert.show("请输入孕产妇证件号码","系统提示");
					return;
				}
				if(!SW302_08.selectedIndex)
				{
					var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
					if(!patternMonther.test(StringUtil.trim(SW302_09.text)))
					{
						Alert.show("孕产妇身份证号码有误!","系统提示");
						return;
					}
				}
				if(!SW302_10.text)
				{
					Alert.show("请输入孕产妇年龄","系统提示");
					return;
				}
				if(!SW302_11.text)
				{
					Alert.show("请输入孕产妇联系方式","系统提示");
					return;
				}
				if(!SW302_19.selectedIndex)
				{
					Alert.show("请输入孕产妇文化程度","系统提示");
					return;
				}
				
				if(!CompMethod.validateAreaCode(SW302_13.getAreaCode(),'4113'))
				{
					Alert.show("现地址选择到最下级","系统提示");
					return ;
				}
				if(!CompMethod.validateAreaCode(SW302_14.getAreaCode(),"4113",true))
				{
					Alert.show("户口地址请选择到乡镇","系统提示");
					return ;
				}
				if(!SW302_15.text)
				{
					Alert.show("请输入孕次","系统提示");
					return;
				}
				if(!SW302_16.text)
				{
					Alert.show("请输入产次","系统提示");
					return;
				}
				if(isNaN(Number(SW302_22.text))||!SW302_22.text)
				{
					Alert.show("请输入正确的胎龄","系统提示");
					return;
				}
				if(!SW302_25_2.selected)//非死胎死产
				{
					
					if(!SW302_20.selectedIndex)
					{
						Alert.show("请选择性别 ","系统提示");
						return;
					}
					if(isNaN(Number(SW302_23.text)))
					{
						Alert.show("请输入正确的出生体重kg","系统提示");
						return;
					}
					
					if(!SW302_21_1.text)
					{
						Alert.show("请输入出生时间","系统提示");
						return;
					}
					var hour:Number=Number(SW302_21_2.text);
					if(isNaN(hour))
					{
						Alert.show("请输入出生时间(时)","系统提示");
						return;
					}
					if(hour<0||hour>23)
					{
						Alert.show("出生时间(时)范围在0-23之间","系统提示");
						return;
					}
					var min:Number=Number(SW302_21_3.text);
					if(isNaN(min))
					{
						Alert.show("请输入出生时间（分）","系统提示");
						return;
					}
					if(min<0||min>59)
					{
						Alert.show("出生时间(分)范围在0-23之间","系统提示");
						return;
					}
				}
				
				if(SW302_83.enabled&&!SW302_83.text)
				{
					Alert.show("请输入产前孕周","系统提示");
					return;
				}
				if(!sw302.sw30229)
				{
					Alert.show("请选择出生缺陷","系统提示");
					return;
				}
				if(SW302_30.enabled&&!sw30229Location[0][0])
				{
					Alert.show("请选择腭裂位置","系统提示");
					return;
				}
				if(SW302_31.enabled&&!sw30229Location[1][0])
