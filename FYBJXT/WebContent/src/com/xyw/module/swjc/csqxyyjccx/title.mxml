			private function getSW30229Val(id:Object):void
			{
				for(var i:uint=0;i<id.numElements;i++)
				{	
					var child:Object=id.getElementAt(i);
					if (child is Container && Container(child).getChildren()) 
						getSW30229Val(child); 
					else if (child is CheckBox&&child.id)
					{
						if(child.selected)					
							sw302.sw30229+=child.automationName+',';
					}
				}
			}
			
			/**
