						<mx:Grid>
							<mx:GridRow>
								<mx:GridItem>
									<s:Label width="59" styleName="myLabel" text="母亲姓名"/><s:TextInput id="E501_03" restrict="^0-9" enabled="false"/>
								</mx:GridItem>
								<mx:GridItem>
