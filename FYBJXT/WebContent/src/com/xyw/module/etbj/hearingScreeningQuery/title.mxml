									<s:Label text="筛查编号" styleName="myLabel"/><s:TextInput id="E501_20"/>
								</mx:GridItem>
								<mx:GridItem>
							<s:Label  text="付费方式 " styleName="myLabel"/>
							<s:HGroup width="129" paddingBottom="2" verticalAlign="middle">
								<s:RadioButton width="70" label="免费" buttonMode="true" groupName="E501_23" id="E501_23_01" selected="true"/>
								<s:RadioButton width="53" label="自费" buttonMode="true" groupName="E501_23" id="E501_23_02"/>
