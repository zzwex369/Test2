								  verticalAlign="middle">
							<s:HGroup verticalAlign="middle">
								<s:Label width="60" text="右      耳  "/>
								<s:RadioButton id="E501_14_01" width="53" label="通过"
											   buttonMode="true" groupName="RightEar"
											   selected="true"/>
								<s:RadioButton id="E501_14_02" width="65" label="未通过"
											   buttonMode="true" groupName="RightEar"/>
								<s:RadioButton id="E501_14_03" width="227" label="未筛"
											   buttonMode="true" groupName="RightEar"/>
							</s:HGroup>
							<s:HGroup width="476" paddingLeft="12" verticalAlign="middle">
