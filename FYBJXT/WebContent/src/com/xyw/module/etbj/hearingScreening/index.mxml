	<fx:Script>
		<![CDATA[
			import com.xyw.module.etbj.model.E501;
			import com.xyw.module.etbj.model.VE501;
			import com.xyw.sys.custom.component.ClearText;
			import com.xyw.sys.custom.component.CompMethod;
			import com.xyw.sys.model.ComboBoxDataRequest;
			import com.xyw.sys.model.SystemUser;
			
			import flashx.textLayout.operations.RedoOperation;
			
			import mx.collections.ArrayCollection;
			import mx.controls.Alert;
			import mx.controls.ProgressBar;
			import mx.events.FlexEvent;
			import mx.formatters.DateFormatter;
			import mx.managers.PopUpManager;
			import mx.rpc.events.ResultEvent;
			import mx.utils.StringUtil;
			[Bindable]
			private var  systemUser:SystemUser=null;
			private var comboBoxDataRequest:ComboBoxDataRequest;
			private var comboBoxDataList:ArrayCollection ;
			private var e501:E501=null;
			private var d60101:Number=0;
			private var e50101:Number;
			private var flag:Boolean=false;
			private var contentLabel:String="听力筛（复）登记表";
			[Bindable]
			private var sexData:ArrayCollection = new ArrayCollection([{label:"请选择",data:0},{label:"男",data:1},{label:"女",data:2},{label:"未知",data:3},{label:"未说明",data:4}]);
			/*****************模块初始化*******************/
			private function init():void
			{
				systemUser=this.parentApplication.systemUser;
				var date:Date=new Date();
				E501_18.selectedDate=date;
				E501_22.selectedDate=date;
				if(this.parentApplication.contentLabel==contentLabel)putVE501();
				if(!flag)this.currentState="";
				initializeComboBox();
			}
			private function initializeComboBox():void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.showPrompt = true;
				comboBoxDataRequest.sql = "select t.d101_01, t.d101_02 from D101 t where t.D101_01 = " + this.systemUser.institutionCode + " or t.D101_11 = " + this.systemUser.institutionCode;
				comboBoxDataRequest.flag = "D101";
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				comboBoxDataList= comboBoxDataResponse.comboBoxDataList;
				var comboBoxDataP:Object=new Object();
				comboBoxDataP.data="0";
				comboBoxDataP.label="其他";
				comboBoxDataList.addItem(comboBoxDataP);
				if(flag == "D101")
				{
					DropSelectedIndex();
				}
			}
			private function DropSelectedIndex(boolean:Boolean=false):void
			{
				var index:int = this.getDropDownListSelectedIndex(comboBoxDataList, this.systemUser.institutionCode);
				if(!boolean)
				{
					this.E501_11.dataProvider = comboBoxDataList;
					this.E501_19.dataProvider = comboBoxDataList;
				}
				this.E501_11.selectedIndex=0;
				this.E501_19.selectedIndex = index;	
			}
			/****************选择对应的信息********************/
			private function getDropDownListSelectedIndex(arrayCollection:ArrayCollection, defaultValue:String,flag:Boolean=false):int
			{
				var len:uint = arrayCollection.length;
				var i:uint = 0;
				var index:int = 0;
				var comboBoxDataP:Object =new Object();
				if(!flag)
				{
					while(i < len) {
						comboBoxDataP= arrayCollection.getItemAt(i);
						if(comboBoxDataP.data == defaultValue) {
							index = arrayCollection.getItemIndex(comboBoxDataP);
							break;
						}
						i++;
					}
				}else if(flag)
				{
					while(i < len) {
						comboBoxDataP = arrayCollection.getItemAt(i);
						if(comboBoxDataP.label == defaultValue) {
							index = arrayCollection.getItemIndex(comboBoxDataP);
							break;
						}
						i++;
					}
				}
				return index;
			}
			/******************更改状态******************/
			private function changeVisible(event:Event):void
			{
				if(ok==event.target){
					hgroup1.visible=true;
				}else if(no==event.target){
					hgroup1.visible=false;
				}
			}
			/*********************读卡***************/
			private function _readCard(event:MouseEvent):void
			{
				try
				{
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						var array:Array = str.split(",");
						var sex:String = array[1];//性别
						if(readCard1==event.target)
							{
							if(sex!="女")
							{
								Alert.show("请读母亲信息！","系统提示");
								return;
							}
							E501_03.text=array[0];
							E501_04.text=array[5]
						}else{						
						if(this.parentFather.selected && sex=="女"){
							Alert.show("请读父亲信息！","系统提示");
							return;
						}else if(this.parentMother.selected && sex=="男"){
							Alert.show("请读母亲信息！","系统提示"); 
							return;
						}
							this.parentName.text = array[0];
							this.num.text = array[5];
							queryDelivery();
					}
					}else{
						Alert.show("读卡失败！");
					}
				} 
				catch(error:Error) 
				{
					Alert.show("读卡失败！");
				}
			} 
			/********************查询分娩信息****************/
			private function queryDelivery():void
			{
				var _name:String=StringUtil.trim(parentName.text);
				var _num:String=StringUtil.trim(num.text);
				try{
					if(this.parentMother.selected){
						if(_name == ""){
							Alert.show("请输入母亲姓名!","系统提示");
							return;
						}
						if(_num == ""){
							Alert.show("请输入母亲证件号码!","系统提示");
							return;
						}
					}else if(this.parentFather.selected){	
						if(_name == ""){
							Alert.show("请输入父亲姓名!","系统提示");
							return;
						}
						if(_num == ""){
							Alert.show("请输入父亲证件号码!","系统提示");
							return;
						}
					}
					var querywindow:QueryWindow = new QueryWindow();
					if(this.parentMother.selected){
						querywindow.d30416 =_name;
						querywindow.d30418 =_num;
					}else if(this.parentFather.selected){
						querywindow.d30422 = _name;
						querywindow.d30424 = _num;
					}
					querywindow.d30411 = this.systemUser.institutionCode;
					CompMethod.popUpTitleWindow(querywindow,this,true);
				}catch(error:Error){
					Alert.show("网络繁忙，请重试!","系统提示");
				}
			}
			/*--------------------加载分娩信息--------------------*/
			public function queryDeliveryRegistHandler(d601:Object):void
			{	
				try
				{
					d60101=d601.d60101;
					E501_03.text=StringUtil.trim(parentName.text);
					E501_04.text=StringUtil.trim(num.text);
					if(d601.d60169!=null){
						E501_05.text=d601.d60169;
					}
					if(d601.d60155!=null){
						E501_06.text=d601.d60155
					};//新生儿姓名
					E501_07.selectedIndex=int(d601.d60140);
					if(d601.d60103!=null){
						E501_08.text=d601.d60103.substring(0,10);
						E501_08H.text = d601.d60103.substring(11,13);
						E501_08M.text= d601.d60103.substring(14,16);
					}
					E501_09.text="";
					E501_09.setStyle("color",0X000000);
					E501_09.text=String(d601.d60122)!="0"?(d601.d60121)+"+"+(d601.d60122):String(d601.d60121);
					E501_10.text=String(d601.d60143/1000);
					E501_11.selectedIndex=this.getDropDownListSelectedIndex(ArrayCollection(E501_11.dataProvider), d601.d60102);
				} 
				catch(error:Error) 
				{
					Alert.show("加载信息失败","系统提示");
					trace("index.queryDeliveryRegistHandler(d601)"+error);
					
				}
			}
			/*******************验证地址选项是否选择到最下级*****************/
			private function validateAreaCode(areaCode:String):Boolean
			{
				if(areaCode == "" || areaCode.length == 0){
					return false;
				} else if(areaCode.length < 9){
					return false;
				} else if(areaCode.length == 9){
					if(areaCode.substr(0,4) == "4113"){
						return false;
					} else {
						return true;
					}
				} else {
					return true;
				}
			}
			/*****************保存验证*******************/
			private function saveE501():void
			{
				e501=new E501();
				if(E501_06.text==""){
					Alert.show("请输入儿童（新生儿）姓名","系统提示");
					return ;
				}
				if(E501_07.selectedIndex==0){
					Alert.show("请选择儿童（新生儿）性别","系统提示");
					return ;
				}
				if(E501_08.text==""){
					Alert.show("请选择出生日期","系统提示");
					return ;
				}
				if(E501_08H.text==""){
					Alert.show("请选择分娩时间小时","系统提示");
					return ;
				}
				if(E501_08M.text==""){
					Alert.show("请选择分娩时间分钟","系统提示");
					return ;
				}
				if(E501_09.text==""||E501_09.text=="例如：39+4或40"){
					Alert.show("请输入孕周","系统提示");
					return ;
				}
				if(E501_11.selectedIndex==0){
					Alert.show("请选择出生医院","系统提示");
					return ;
				} 
				if(E501_10.text==""){
					Alert.show("请输入出生体重","系统提示");
					return ;
				}
				if(E501_03.text==""){
					Alert.show("请输入家长姓名","系统提示");
					return ;
				}
				if(E501_04.text==""){
					Alert.show("请输入身份证号","系统提示");	
					return ;
				}
				if(E501_05.text==""){
					Alert.show("请输入联系电话","系统提示");
					return ;
				}
				if(E501_20.text==""){
					Alert.show("请输入筛查编号","系统提示");
					return ;
				}
				E501_12_01.selected?e501.e50112="1":e501.e50112="2";
				if(E501_13_01.selected){
					e501.e50113="1";
				}
				if(E501_13_02.selected){
					e501.e50113="2";
				}
				if(E501_14_01.selected){
					e501.e50114="1";
				}
				if(E501_14_02.selected){
					e501.e50114="2";
				}
				if(E501_14_03.selected){
					e501.e50114="3";
				}
				if(E501_15_01.selected){
					e501.e50115="1";
				}
				if(E501_15_02.selected){
					e501.e50115="2";
				}
				if(E501_15_03.selected){
					e501.e50115="3";
				}
				if(E501_16_01.selected){
					e501.e50116="1";
				}
				if(E501_16_02.selected){
					e501.e50116="2";
				}
				if(E501_16_03.selected){
					e501.e50116="3";
				}
				if(E501_17.text==""){
					Alert.show("请输入筛查者签名","系统提示");
					return ;
				}
				if(E501_18.text==""){
					Alert.show("请选择筛查日期","系统提示");
					return ;
				}
				if(E501_21.text==""){
					Alert.show("请输入录入人员","系统提示");
					return ;
				}
				if(E501_22.text==""){
					Alert.show("请选择录入时间","系统提示");
					return ;
				}
				E501_23_01.selected?e501.e50123="1":e501.e50123="2";
				if(!validateAreaCode(E501_24.getAreaCode()))
				{
					Alert.show("母亲地址选择到最下级","系统提示");
					return ;
				}
				_saveE501();
			}
			/***************保存发送数据*********************/
			private function _saveE501():void
			{
				try
				{
					e501.e50102=d60101;
					e501.e50103=StringUtil.trim(E501_03.text);
					e501.e50104=StringUtil.trim(E501_04.text);
					e501.e50105=StringUtil.trim(E501_05.text);
					e501.e50106=StringUtil.trim(E501_06.text);
					e501.e50107=E501_07.selectedItem.data;	
					e501.e50108=DateFormatter.parseDateString(E501_08.text+" "+E501_08H.text+":"+E501_08M.text);
					e501.e50109=StringUtil.trim(E501_09.text);
					e501.e50110=Number(StringUtil.trim(E501_10.text));
					e501.e50111=E501_11.selectedItem.data;
					e501.e50117=StringUtil.trim(E501_17.text);
					e501.e50118=DateField.stringToDate(this.E501_18.text, "YYYY-MM-DD");
					e501.e50119=systemUser.institutionCode;
					e501.e50120=StringUtil.trim(E501_20.text);
					e501.e50121=StringUtil.trim(E501_21.text);
					e501.e50122=DateField.stringToDate(this.E501_22.text, "YYYY-MM-DD");
					e501.e50124=E501_24.getAreaCode();
					if(!flag){
						e501Service.saveE501(e501);
					}else{
						e501.e50101=e50101;
						save.enabled=false;
						e501Service.renewE501(e501);
					}
				} 
				catch(error:Error) 
				{
					Alert.show("保存失败","系统提示");
					trace("index._saveE501()"+error);
					
				}
			}
			/*****************保存返回的信息*******************/
			private function saveE501Handler(event:ResultEvent):void
			{
				var e501Response:Object=event.result;
				if(!e501Response.state){
					Alert.show(e501Response.errorMessage,"系统提示");
					return ;
				}
				Alert.show(e501Response.promptMessage,"系统提示");
				clearE501();
			}
			/*****************更新返回的信息*******************/
			private function renewE501Handler(event:ResultEvent):void
			{
				var e501Response:Object=event.result;
				if(!e501Response.state){
					Alert.show(e501Response.errorMessage,"系统提示");
					return ;
				}
				Alert.show(e501Response.promptMessage,"系统提示");
			}
			/*******************得到需要修改的信息*****************/
			private function putVE501():void
			{	
				try
				{
					var ve501:Object=this.parentApplication.obj;
					if(ve501!=null){
						e50101=ve501.e50101;
						d60101=ve501.e50102;
						E501_03.text=ve501.e50103;
						E501_04.text=ve501.e50104;
						E501_05.text=ve501.e50105;
						E501_06.text=ve501.e50106;
						E501_07.selectedIndex=int(ve501.e50107);
						E501_08.text=ve501.e50108.substring(0,10);
						E501_08H.text = ve501.e50108.substring(11,13);
						E501_08M.text= ve501.e50108.substring(14,16);
						E501_09.text=ve501.e50109;
						E501_10.text=String(ve501.e50110);
					//	E501_11.text=ve501.e50111;
						E501_20.text=ve501.e50120;
						ve501.e50112=="1"?E501_12_01.selected=true:E501_12_02.selected=true;
						ve501.e50113=="1"?E501_13_01.selected=true:E501_13_02.selected=true;
						if(ve501.e50114=="1")
						{
							E501_14_01.selected=true;
						}
						if(ve501.e50114=="2")
						{
							E501_14_02.selected=true;
						}
						if(ve501.e50114=="3")
						{
							E501_14_03.selected=true;
						}
						if(ve501.e50115=="1")
						{
							E501_15_01.selected=true;
						}
						if(ve501.e50115=="2")
						{
							E501_15_02.selected=true;
						}
						if(ve501.e50115=="3")
						{
							E501_15_03.selected=true;
						}
						if(ve501.e50116=="1")
						{
							E501_16_01.selected=true;
						}
						if(ve501.e50116=="2")
						{
							E501_16_02.selected=true;
						}
						if(ve501.e50116=="3")
						{
							E501_16_03.selected=true;
						}
						E501_17.text=ve501.e50117;
						E501_18.text=ve501.e50108Str;
						//E501_19.text=ve501.e50119Zh;
						E501_21.text=ve501.e50121;
						E501_22.text=ve501.e50122Str;
						this.parentApplication.obj=null;
						this.currentState="special";
						save.enabled=true;
						flag=true;
					}
				} 
				catch(error:Error) 
				{
					Alert.show("加载数据异常","系统提示");
					trace("index.putVE501()"+error);
				}
			}
			private function changeText(event:FocusEvent):void
			{
				if(E501_09.text=="例如：39+4或40")
				{
					E501_09.text="";
					E501_09.setStyle("color",0X000000);
				}
			}
			private function clearE501():void
			{
				var clearText:ClearText=new ClearText();
				clearText._clearText(this,"E501_22",true);
				DropSelectedIndex(true);
				E501_24.setAreaCode(systemUser.institutionCode.substring(0,4));
				E501_07.selectedIndex=0;
				E501_23_01.selected=true;
				E501_12_01.selected=true;
				E501_13_01.selected=true;
				E501_14_01.selected=true;
				E501_15_01.selected=true;
				E501_16_01.selected=true;
			}
			private function close():void
			{
				var progressBar1:ProgressBar=this.parentApplication.progressBar1 as ProgressBar;
				var superTab:Object=this.parentApplication.superTabNavigator;
				for(var i:uint = 0; i <superTab.numChildren; i++) {
					var vbox:Object =superTab.getChildAt(i);
					var vboxLabel:String = vbox.label;
					if(vboxLabel == contentLabel) {
						superTab.removeChildAt(i);
						break;
					}
				}
				this.parentApplication.loadModule("com/xyw/module/etbj/hearingScreening/index.swf");
			}
		]]>
	</fx:Script>
	<s:Scroller width="100%" height="100%" horizontalScrollPolicy="auto" verticalScrollPolicy="auto">
		<s:VGroup>
			<mx:TabNavigator id="tab1" chromeColor="#ffffff" creationPolicy="all"  width.special="0" height.special="0">
				<s:NavigatorContent width="1140" label="分娩确认">
					<s:VGroup paddingBottom="10" paddingLeft="10" paddingRight="10">
						<s:HGroup width="100%" height="100%" verticalAlign="middle">
							<s:Label text="是否登记分娩信息"/>
							<s:RadioButton id="ok" label="是" buttonMode="true"
										   click="changeVisible(event)" groupName="qf"
										   selected="true"/>
							<s:RadioButton id="no" label="否" buttonMode="true"
										   click="changeVisible(event)" groupName="qf"/>
							<s:HGroup id="hgroup1" verticalAlign="middle">
								<s:Label text="分娩登记表查询条件    "/>
								<s:RadioButton id="parentMother" label="母亲" buttonMode="true"
											   groupName="fm" selected="true"/>
								<s:RadioButton id="parentFather" label="父亲" buttonMode="true"
											   groupName="fm"/>
								<s:Label paddingLeft="8" text="请输入姓名"/><s:TextInput id="parentName"/>
								<s:Label text="证件号码"/><s:TextInput id="num" width="140"/>
								<s:Button label="查询" buttonMode="true" click="queryDelivery()"/>
								<s:Button id="readCard" width="60" label="读卡" buttonMode="true"
										  click="_readCard(event)"  enabled="{systemUser.duka=='1'}"/>	
							</s:HGroup>
						</s:HGroup>
					</s:VGroup>
				</s:NavigatorContent>
			</mx:TabNavigator>
			
			<mx:TabNavigator chromeColor="#ffffff" creationPolicy="all">
