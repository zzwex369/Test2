						var year:Number = Number(this.E502_09.text.substr(6,4));
						var month:Number = Number(this.E502_09.text.substr(10,2));
						var day:Number = Number(this.E502_09.text.substr(12,2));
						var date:Date = new Date();
						var nowMonth:Number = date.month + 1;
						var age:Number = date.fullYear - year;
						if(month > nowMonth || month == nowMonth && day > date.date){
							age=age - 1;;
						}
						E502_10.text=age+"";
					}
				}
			}
