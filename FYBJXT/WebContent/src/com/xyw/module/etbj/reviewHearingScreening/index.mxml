		comboBoxDataRequest.showPrompt = true;
		comboBoxDataRequest.sql = "select * from Z502_04 t";
		comboBoxDataRequest.flag = "Z502_04";
		this.systemService.getComboBoxData(this.comboBoxDataRequest);
	}
	/******************得到数据******************/
	private function getComboBoxDataHandler(event:ResultEvent):void
	{
		var comboBoxDataResponse:Object =  event.result;
		var flag:String = comboBoxDataResponse.flag;
		var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
		if(flag == "D101")
		{
			var index:int = this.getDropDownListSelectedIndex(comboBoxDataList, this.systemUser.institutionCode);
			this.E502_31.dataProvider = comboBoxDataList;
			this.E502_21.dataProvider = comboBoxDataList;
			this.E502_31.selectedIndex = index;	
			this.E502_21.selectedIndex=index;
		}else if(flag=="Z502_04")
		{
			E502_27.dataProvider=comboBoxDataList;
			E502_27.selectedIndex=0;
			E502_27.dispatchEvent(new Event("addData",true,false));
		}
	}
	/****************选择对应的信息********************/
	private function getDropDownListSelectedIndex(arrayCollection:ArrayCollection, defaultValue:String):int
	{
		var len:uint = arrayCollection.length;
		var i:uint = 0;
		var index:int = 0;
		while(i < len) {
			var comboBoxDataP:Object = arrayCollection.getItemAt(i);
			if(comboBoxDataP.data == defaultValue) {
				index = arrayCollection.getItemIndex(comboBoxDataP);
				break;
			}
			i++;
		}
		return index;
	}
	/*****************查询首次筛查*******************/
	private function query():void
	{
		var _name:String=StringUtil.trim(parentName.text);
		var _num:String=StringUtil.trim(num.text);
		if(_num==""){
			Alert.show("请输入正确的证件号","系统提示");
			return;
		}
		try{
		var querywindow:QueryWindow = new QueryWindow();
		querywindow.e50103 =_name;
		querywindow.e50104=_num;
		CompMethod.popUpTitleWindow(querywindow,this,true);
	}catch(error:Error){
		Alert.show("网络繁忙，请重试!","系统提示");
	}
	}
	/*********************读卡***************/
	private function _readCard(event:MouseEvent):void
	{
		try
		{
			var str:String = ExternalInterface.call("parent.readCard");
			if(str != null && str.length != 0){
				var array:Array = str.split(",");
				var sex:String = array[1];//性别
				if(readCard==event.target)
				{
					this.parentName.text = array[0];
					this.num.text = array[5];
					query();
				}else if(readCard2==event.target)
				{						
					if("男"==sex)
					{
						Alert.show("请读母亲的身份证信息","系统提示");
						return ;
					}else
					{
						E502_05.text=array[0];
						E502_06.text=array[5];
						setE502_07(event)
					}
				}else if(readCard3==event.target)
				{
					if("女"==sex)
					{
						Alert.show("请读父亲的身份证信息","系统提示");
						return ;
					}else
					{
						E502_08.text=array[0];
						E502_09.text=array[5];
						setE502_10(event)
					}
				}
			}else{
				Alert.show("读卡失败！");
			}
		} 
		catch(error:Error) 
		{
			Alert.show("读卡失败！");
		}
	}
