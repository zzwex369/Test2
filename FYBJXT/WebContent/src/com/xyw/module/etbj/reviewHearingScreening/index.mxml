	/****************年龄事件********************/
	private function _addEventListener():void
	{
		E502_06.addEventListener(FlexEvent.CHANGING,setE502_07);
		E502_09.addEventListener(FlexEvent.CHANGING,setE502_10);
		E502_27.addEventListener("addData",setE502_27);
	}
	/*****************母亲年龄*******************/
	private function setE502_07(event:Event):void{
			var cardId:String  = E502_06.text;
			if(cardId.length >= 16){
				var year:Number = Number(E502_06.text.substr(6,4));
				var month:Number = Number(E502_06.text.substr(10,2));
				var day:Number = Number(E502_06.text.substr(12,2));
				var date:Date = new Date();
				var nowDay:Number = date.date;
				var nowMonth:Number = date.month + 1;
				var nowYear:Number = date.fullYear;
				var age:Number;
				if(nowMonth < month){
					age = nowYear - year - 1;
				}else if(nowMonth == month){
					if(day > nowDay){
						age = nowYear - year -1;
					}else{
						age = nowYear - year;
					}
				}else if(nowMonth > month){
					age = nowYear - year;
				}
				E502_07.text = age + "";
			}
		}
	/*******************父亲年龄*****************/
	private function setE502_10(event:Event):void
	{
			var cardId:String =E502_09.text;
			if(cardId != ""){
				if(cardId.length >= 16){
					var year:Number = Number(this.E502_09.text.substr(6,4));
					var month:Number = Number(this.E502_09.text.substr(10,2));
					var day:Number = Number(this.E502_09.text.substr(12,2));
					var date:Date = new Date();
					var nowMonth:Number = date.month + 1;
					var age:Number = date.fullYear - year;
					if(month > nowMonth || month == nowMonth && day > date.date){
						age=age - 1;;
					}
					E502_10.text=age+"";
			}
		}
	}
