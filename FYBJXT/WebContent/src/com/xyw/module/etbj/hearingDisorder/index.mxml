			private function _readCard(event:MouseEvent):void
			{
				try
				{
					var str:String = ExternalInterface.call("parent.readCard");
					if(str != null && str.length != 0){
						var array:Array = str.split(",");
						var sex:String = array[1];//性别
						cardId=array[5];
						if(event.target!=readCard3&&"男"==sex)
						{
							Alert.show("请读母亲的身份证信息","系统提示");
							return ;
						}
						if(readCard==event.target)
						{	
							this.parentName.text = array[0];
							this.num.text = array[5];
							queryHearingScreening();
						}else if(readCard2==event.target)
						{						
							
								E503_17.text=array[0];
								E503_18.text=cardId;
								E503_19.text=getAge(cardId);
						}else if(readCard3==event.target)
						{
							if("女"==sex)
							{
								Alert.show("请读父亲的身份证信息","系统提示");
								return ;
							}else
							{
								E503_13.text=array[0];
