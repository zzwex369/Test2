					Alert.show("读卡失败！");
				}
			}
			/*******************查询听力登记表*****************/
			private function queryHearingScreening():void
			{
				var _name:String=StringUtil.trim(parentName.text);
				var _num:String=StringUtil.trim(num.text);
				if(_num==""){
					Alert.show("请输入正确的证件号","系统提示");
					return;
				}
				try{
					var querywindow:QueryWindow = new QueryWindow();
					querywindow.e50103 =_name;
					querywindow.e50104=_num;
					CompMethod.popUpTitleWindow(querywindow,this,true);
				}catch(error:Error){
					Alert.show("网络繁忙，请重试!","系统提示");
				}
			}
