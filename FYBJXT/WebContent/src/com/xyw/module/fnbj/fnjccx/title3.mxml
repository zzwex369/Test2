			private function setYunZhou(event:Event):void
			{
				var yuejing:Number = F303_22.selectedDate.time; //末次月经
				var jiance:Number = F303_14.selectedDate.time; //检查日期
				var tian:int = (jiance - yuejing)/(1000*60*60*24);
				if(tian>0){
					tian = tian + 1;
					this.F303_23.text = Math.floor(tian/7)+"";   //建卡孕周
					this.F303_24.text = tian%7+"";
				}
				var year:Number =Number(F303_22.text.substr(0,4));
				var month:Number =0;
				var day:Number =Number(F303_22.text.substr(8,2)) +7;
				var month1:Number =Number(F303_22.text.substr(5,2));
				if(month1==1||month1==3||month1==5||month1==7||month1==8||month1==10||month1==12){
					if(day>31){
						month1=month1+1;
						day=day-31
					}
				}else if(month1==4||month1==6||month1==9||month1==11){
					if(day>30){
						month1=month1+1;
						day=day-30;
						if(month1>3){
							month1=month1-3;
							year=year+1;
						} 
					}
				}else{
					if(year%4==0){
						if(day>29){
							month1=month1+1;
							day=day-29;
						} 
					}else{
						if(day>28){
							month1=month1+1;
							day=day-28;
						} 
					}
				}
				if(month1>3){
					month1=month1-3;
					year=year+1;
				}else{
					month1=month1+9;
				} 
				this.F303_25.text =year+"-"+month1+"-"+day; 
			}
