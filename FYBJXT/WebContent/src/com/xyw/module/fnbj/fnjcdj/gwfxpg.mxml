	<fx:Script>
		<![CDATA[
			import com.xyw.sys.custom.component.CompMethod;
			
			import mx.collections.ArrayCollection;
			import mx.collections.Sort;
			import mx.core.Container;
			import mx.events.FlexEvent;
			
			import spark.components.CheckBox;
			private var label:Object;//当前label
			private var gridItemLast:Object;//得到当前行最后一格
			private var gridRow:Object;//得到行
			private var allColor:Array=[new Array(),new Array(),new Array(),new Array(),new Array()];//全部内容1 绿色 2.黄 3 橙 4红  5紫
			private var arrCheck:Array=[null];//选中的病情 1 内容 2 数字 用于排序
			private var blankReg:RegExp = /\s/g; //匹配空格
			private var numReg:RegExp = /^\d\.\d+\.?\d?/; //匹配序号
			private var k:int=0;
			[Bindable]
			[Embed(source="images/first.png")]
			private var firstIcon:Class;
			
			[Bindable]
			[Embed(source="images/next.png")]
			private var nextIcon:Class;
			
			[Bindable]
			[Embed(source="images/previous.png")]
			private var previousIcon:Class;
			private function mouseOverHandler(event:MouseEvent):void
			{
				getGridRow(event);
				gridItemLast=getGridItem(1);
				label=event.target;
				setRowStyle('#cccccc');
			}
			
			private function mouseOutHandler(event:MouseEvent):void
			{
				setRowStyle('#ffffff');
			}
			
			/**
			 *点击
			 */
			private function clickHandler(event:MouseEvent):void
			{
				if(event.target is CheckBox )
					return ;
				var gridItem:Object=getGridItem(1) ;
				var checkBox:Object=gridItem.getElementAt(0);
				checkBox.selected=!checkBox.selected;
			}
			
			/**
			 *选择框悬浮
			 */
			private function rollOverHandler(event:MouseEvent):void
			{
				gridRow=event.target.parent;
				var gridItemFirst:Object=getGridItem(2) ;
				label=gridItemFirst.getElementAt(0);
				gridItemLast=event.currentTarget;
				setRowStyle('#cccccc');
			}
			
			/**
			 *选择框离开
			 */
			private function rollOutHandler(event:MouseEvent):void
			{
				setRowStyle('#ffffff');
			}
			
			/**
			 *得到行
			 */
			private function getGridRow(event:MouseEvent):Object
			{
				return gridRow=event.target.parent.parent;
			}
			
			/**
			 *得到格
			 */
			private function getGridItem(i:int):Object
			{
				return gridRow.getElementAt (gridRow.numElements-i);
			}
			
			/**
			 *设置鼠标状态颜色
			 */
			private function setRowStyle(color:String):void
			{
				label.setStyle('backgroundColor',color);
				gridItemLast.setStyle('backgroundColor',color);
			}
			
			/**
			 *切换页面
			 */
			private function button_clickHandler(event:MouseEvent):void
			{
				var flag:Boolean=getCheckBox(vgroup);
				if(flag)
				{
					if(event.target==save)
					{
						close();
						return ;
					}
					k=0;
					for(var i:int=0;i<this.states.length;i++)
					{
						if(this.states[i].name==this.currentState)
						{
							if(event.target==next)
								this.currentState=this.states[++i].name
							else if(event.target==previous)
								this.currentState=this.states[--i].name
							else if(event.target==first)
								this.currentState="";
							setNum(1,true);
							break;
						}
					}		
				}
			}
			
			
			/**
			 *设置序号  删除数据
			 */
			private function setNum(numObj:Object,flag:Boolean=false):int
			{
				var i:int=0;
				if(!numObj)
				{
					if(this.currentState!="yellow")
						i=4;
				}else{
					if(this.currentState=="yellow")
					{
						++i;
						deleteArray(0,flag);
					}
					else if(this.currentState=="orange")
						i=2;
					else if(this.currentState=="red")
					{
						i=3;
						deleteArray(4,flag);	
					}
					deleteArray(i,flag);
				}
				return i;
			}
			
			/**
			 *跳转删除数据
			 */
			private function deleteArray(i:int,flag:Boolean=false):void
			{
				if(allColor[i].length&&flag)
					allColor[i].splice(0,allColor[i].length);
			}
			
			/**
			 *深复制调用的方法
			 */
			private	function clone(source:Object):Object
			{
				var myBA:ByteArray = new ByteArray();
				myBA.writeObject(source);
				myBA.position = 0;
				return(myBA.readObject());
			}
			
			
			/**
			 *得到选中的那一条记录
			 */
			protected function getCheckBox(ui:Object):Boolean
			{
				for(var i:int=0;i<ui.numElements;i++)
				{
					var child:Object=ui.getElementAt(i);
					if(child is Container && Container(child).getChildren())
						getCheckBox(child);				
					else if(child is CheckBox)
					{
						if(CheckBox(child).selected)
						{
							var j:int;
							gridRow=child.parent.parent;
							var gridItemFirst:Object=getGridItem(2) ;
							label=gridItemFirst.getElementAt(0);
							var text:String=label.text.toString();
							var numObj:Object=text.match(numReg);
							text=!numObj?"1."+text:text.replace(numReg,++k+'.');
							text=text.replace(blankReg,'')
							arrCheck[0]=text;
							j=setNum(numObj);
							allColor[j].push(clone(arrCheck));
						}
						
					}
				}
				return true;
			}
			
			private function close(flag:Boolean=false):void
			{
				if(flag)
					getCheckBox(vgroup);				
				var index:Object=this.owner;
				index.getColor(this.allColor);
				CompMethod.closeWindow(this);
			}
		]]>
	</fx:Script>
