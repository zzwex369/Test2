			private function saveOrRenewF604Handler(event:ResultEvent):void
			{
				var f604Response:Object =event.result;
				if(!f604Response.state){
					this.saveButton.enabled=true;
					Alert.show(f604Response.errorMessage,"系统提示");
					return;
				}
				Alert.show(f604Response.promptMessage,"系统提示");
				var index:Object = this.owner ;
				index.refresh();
				this.removeWindow();
				
			}
			
			/**
			 * 
			 */
			private function changeHandler(event:Event,arr:Array,id:Object=null):void
			{
				var value:String=id?(StringUtil.trim(id.text)?'1':'2'):event.target.value;
				for(var i:Object in arr)
				{
					if(value=="2")
						CompMethod._walk(arr[i]);
					else
						walk(arr[i],null,0,'1');
				}
			}
			
