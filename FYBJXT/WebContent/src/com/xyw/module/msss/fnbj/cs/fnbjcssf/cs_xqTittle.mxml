			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				--count;
				var comboBoxDataResponse:Object =event.result;
				var flag:String =comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection=comboBoxDataResponse.comboBoxDataList;
				if(flag =="D101"){
					var index:int =CompMethod.getDropDownListSelectedIndex(comboBoxDataList,this.systemUser.institutionCode);
					this.F604_55.dataProvider =this.F604_59.dataProvider=comboBoxDataList;
					this.F604_55.selectedIndex =this.F604_59.selectedIndex=index;
				}else if(flag == "Z503_16")
				{
					this.F604_53.dataProvider = comboBoxDataList;
					this.F604_53.selectedIndex = 0;
				}else if(flag == "Z503_17")
				{
					this.F604_54.dataProvider = comboBoxDataList;
					this.F604_54.selectedIndex = 0;
				}else if(flag == "S301_10_01")
				{
					this.F604_52.dataProvider = comboBoxDataList;
					this.F604_52.selectedIndex = 0;
				}else if(flag == "S301_10_02")
				{
					this.F604_56.dataProvider = comboBoxDataList;
					this.F604_56.selectedIndex = 0;
				}
				if(!count&&vf604)
					callLater(putVf604);
			}
			
			/**
			 *模块显示 隐藏
			 */
			private function setEnable(status:Boolean=false):void
			{
				for(var i:int=2;i<vgroup.numElements;i++)
				{
					var tab:Object=vgroup.getElementAt(i);
					if(tab is TabNavigator)
					{
						if(status)
							tab.enabled=status;
						else
						{
							if(i<(vgroup.numElements-2))
								tab.enabled=status;
						}
					}else
						continue;	
				}
			}
			
			/**
			 * 得到单选框值
			 * idArr 组件数组
			 * index 下标 
			 * 
			 */
			private function getRadioButtonValue(idArr:Array,index:int=0):String
			{
				var value:String='';
				for(var i:int=0;i<idArr[index].numElements;i++)
				{
					var radioButton:Object=idArr[index].getElementAt(i);
					if(radioButton is RadioButton)
					{
						if(radioButton.selected)
						{
							value=radioButton.value.toString();
							break;
						}
					}else
						continue;
					
					
				}
				return value?value:getRadioButtonValue(idArr,++index);
				
			}
			
			
			/**
			 * 递归组件得到单选框值
			 * ui 组件Id
			 * index 得到按钮值放入对应字段中 
			 * param 有值   选择按钮
			 * 		               得到选择按钮的值
			 */
			private  function walk(ui:Object,arr:Array,index:int,param:String=''):void
			{ 
				for(var i:uint=0;i<ui.numElements;i++)
				{	
					var child:Object=ui.getElementAt(i);
					if (child is Container && Container(child).getChildren()||child is GroupBase) { 
						walk(child,arr,index,param); 
					} else if(child is RadioButton){
						if(param)
						{
							if(child.value.toString()==param)
							{
								child.selected=true;
								break;							
							}
							else
								continue;
						}else
						{
							if(child.selected)
							{
								arr[index]=child.value.toString();
								break;
							}else
								continue;
						}	
					}
				}	
			}
			
			/**
			 * 验证输入框，下拉框
			 * param 0  实际值
			 *       1 比对值
			 * status 1 text 类型
			 *        2    下拉框类型
			 * ID    组件标志
			 * str    提示信息
			 * 
			 * 为空或者选择0 return true
			 * 				
			 */
			private  function validate(param:Array,status:String,id:Object,str:String):Boolean
			{		
					if(param&&(param[0]===undefined||(param[0]!=param[1])))
						return false;
					if("1"==status)
					{
						if(StringUtil.trim(id.text)=="")
						{
							Alert.show("请输入"+str,"系统提示");
							return true;
						}
						
					}else if("2"==status)
					{
						if(!id.selectedIndex)
						{
							Alert.show("请选择"+str,"系统提示");
							return true;
						}
					}
			
				return false;
			}
			
			/**
			 *
			 */
			private function _validate():void
			{
				var index:int=F604_54.selectedIndex;
				if(fileStatus)//没有档案
				{
					if(!StringUtil.trim(F604_04.text))
					{
						Alert.show("请输入姓名","系统提示");
						return;
					}
					if(!StringUtil.trim(F604_05.text))
					{
						Alert.show("请输入身份证号","系统提示");
						return;
					}
					if(StringUtil.trim(F604_05.text))
					{
					     var patternMonther:RegExp = /^(\d{18,18}|\d{15,15}|\d{17,17}X|\d{17,17}x)$/g;
						if(!patternMonther.test(StringUtil.trim(F604_05.text)))
						{
							Alert.show("身份证号码有误!","系统提示");
							return;
						}
					}
					if(!StringUtil.trim(F604_06.text))
					{
						Alert.show("请输入年龄","系统提示");
						return;
					}
					if(!StringUtil.trim(F604_07.text))
					{
						Alert.show("请输入末次月经","系统提示");
						return;
					}
					if(!StringUtil.trim(F604_08.text))
					{
						Alert.show("请输入超声号","系统提示");
						return;
					}
					if(!StringUtil.trim(F604_09.text))
					{
						Alert.show("请输入血清编号","系统提示");
						return;
					}
				}
				
				
				if(!StringUtil.trim(F604_52.text))
				{
					Alert.show("请输入随访人员","系统提示");
					return;
				}
				
				if(!StringUtil.trim(F604_51.text))
				{
					Alert.show("请输入随访日期","系统提示");
					return;
				}
				
				if(!F604_53.selectedIndex)
				{
					Alert.show("请选择随访方式","系统提示");
					return;
				}
				
				if(!index)
				{
					Alert.show("请选择随访结果","系统提示");
					return;
				}
				if(index==1||index==2)
					saveF604(false);
				else if(index>2)//已检查或者已随访
				{
					this.walk(this.F604_60,bfzArr,0);
					if(bfzArr[0]=='1')//
					{
						bfzArr[1]=getRadioButtonValue([F604_14,F604_14_1]);
						bfzArr[2]=getRadioButtonValue([F604_15]);
					}
					
					this.walk(this.F604_22,csqxArr,0);//出生缺陷
					this.walk(this.F604_47,csqxArr,9);//染色体异常
					if(csqxArr[0]=='1')//
					{
						this.walk(this.F604_24,csqxArr,1);//心血管
						this.walk(this.F604_27,csqxArr,2);//神经系统
						this.walk(this.F604_30,csqxArr,3);//颜面部畸形
						this.walk(this.F604_33,csqxArr,4);//胸部
						this.walk(this.F604_36,csqxArr,5);//泌尿系统
						this.walk(this.F604_39,csqxArr,6);//骨骼
						this.walk(this.F604_42,csqxArr,7);//消化系统
						this.walk(this.F604_44,csqxArr,8);//宫内死胎
						if(validate([csqxArr[1],'9'],'1',F604_25,'心血管其他'))
							return ;
						if(validate([csqxArr[2],'9'],'1',F604_28,'神经系统其他'))
							return ;
						if(validate([csqxArr[3],'9'],'1',F604_31,'颜面部畸形其他'))
							return ;
						if(validate([csqxArr[4],'9'],'1',F604_34,'胸部畸形其他'))
							return ;
						if(validate([csqxArr[5],'9'],'1',F604_37,'泌尿系统其他'))
							return ;
						if(validate([csqxArr[6],'9'],'1',F604_40,'骨骼系统其他'))
							return ;
						if(validate([csqxArr[7],'9'],'1',F604_43,'消化系统其他'))
							return ;	
					}
					
					if(csqxArr[9]=='1')
					{
						this.walk(this.F604_49,csqxArr,10);
						if(validate(null,'1',F604_50,'确诊医疗机构'))
							return ;
						if(validate(null,'1',F604_48,'具体描述'))
							return ;
					}
					saveF604();
				}
			}
				
