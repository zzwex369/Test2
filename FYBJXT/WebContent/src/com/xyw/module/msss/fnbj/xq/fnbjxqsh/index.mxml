			private function keyUp_Handler(event:KeyboardEvent):void
			{
				var id:String=(Object(focusManager.getFocus()).id)+"";
				if(this.currentState=="querySingle"&&id!='f60155')
				{
					var flag:Boolean=false;
					var state:String="pass";
					if(event.keyCode==Keyboard.ENTER)
					{
						flag=true;
						
					}else if(event.keyCode==Keyboard.ESCAPE)
					{
						flag=true;
						state="noPass";
					}
					if(flag&&vF601.f60159=='2'&&!pop)
					{
						pop=true;				
						_batch(state,vf601Arr,"scan");
					}
					
				
				}
				
			}
			
			public function queryF60155(f60155:String):void
			{
				pop=false;
				f601Service.queryByF60155(f60155);	
			}
			
		
			
			
			
			private function mouseEvent_Handler(event:MouseEvent):void
			{
				var id:Object=event.currentTarget.id;
				if(this.currentState=="querySingle"&&(id!='f60155'))
					this.setFocus();
				else if(id=='f60155')
					f60155.setFocus();
			}
			
			
		]]>
