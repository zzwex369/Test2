			public var systemUser:SystemUser = null;
			public var f601Request:F601Request;
			[Bindable]
			public var vF601:Object = null;
			[Bindable] 
			private var f601Arr:ArrayCollection = new ArrayCollection(); 
			[Bindable] 
			private var vf601Arr:ArrayCollection = null; 
			[Bindable]
