<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   xmlns:hi="http://hi.csdn.net/To9m"
			   xmlns:component="com.xyw.sys.custom.component.*"
			   backgroundColor="white"  width="568" height="630"
			   close="removeWindow()" cornerRadius="5" 
			   skinClass="com.xyw.sys.custom.skin.CustomTitleWindow"
			   title="超声申请报告单">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<fx:Style>
		@namespace s "library://ns.adobe.com/flex/spark";
		@namespace mx "library://ns.adobe.com/flex/mx";
		@namespace skin "com.xyw.sys.custom.skin.*";
		.textInput{
			skinClass:ClassReference("com.xyw.sys.custom.skin.CustomTextInputSkin");
		}
		mx|GridItem{
			verticalAlign:middle;
		}
	</fx:Style>
	<fx:Script>
		<![CDATA[
			import com.xyw.module.msss.fnbj.model.VF501;
			
			import mx.managers.PopUpManager;
			import mx.printing.FlexPrintJob;
			import mx.printing.FlexPrintJobScaleType;
			[Bindable]
			public var vf501:Object;
			private function printF308():void
			{
				var flexPrintJob:FlexPrintJob = new FlexPrintJob();
				flexPrintJob.printAsBitmap = false;
				if(flexPrintJob.start())
				{
					flexPrintJob.addObject(this.data,FlexPrintJobScaleType.MATCH_WIDTH);
					flexPrintJob.send();
				}
				
			}
			public function load():void{
				if(vf501.f50109!=null){
					if(vf501.f50109=="1")this.f501_09_01.selected = true;
					if(vf501.f50109=="2")this.f501_09_02.selected = true;
				}
				if(vf501.f50110!=null){
					if(vf501.f50110=="1")this.f501_10_01.selected = true;
					if(vf501.f50110=="2")this.f501_10_02.selected = true;
				}
				if(vf501.f50111!=null){
					if(vf501.f50111=="1")this.f501_11_01.selected = true;
					if(vf501.f50111=="2")this.f501_11_02.selected = true;
				}
				if(vf501.f50112!=null){
					if(vf501.f50112=="1")this.f501_12_01.selected = true;
					if(vf501.f50112=="2")this.f501_12_02.selected = true;
				}
				if(vf501.f50114!=null){
					if(vf501.f50114=="1")this.f501_14_01.selected = true;
					if(vf501.f50114=="2")this.f501_14_02.selected = true;
					if(vf501.f50114=="3")this.f501_14_03.selected = true;
				}
				var array:Array = vf501.f50119.split(",");
				for(var i:int=0;i<array.length;i++){
					if(array[i]=="1"){
						this.f501_19_01.selected = true;
					}
					if(array[i]=="2"){
						this.f501_19_02.selected = true;
					}
					if(array[i]=="3"){
						this.f501_19_03.selected = true;
					}
					if(array[i]=="4"){
						this.f501_19_04.selected = true;
					}
					if(array[i]=="5"){
						this.f501_19_05.selected = true;
					}
					if(array[i]=="6"){
						this.f501_19_06.selected = true;
					}
					if(array[i]=="7"){
						this.f501_19_07.selected = true;
					}
					if(array[i]=="8"){
						this.f501_19_08.selected = true;
					}
				}
			}
			private function removeWindow():void{
				PopUpManager.removePopUp(this);
			}
		]]>
	</fx:Script>
	<s:VGroup  paddingBottom="10">
		<s:VGroup width="568" gap="10"  paddingBottom="10" id="data">
			<s:HGroup width="100%">
				<s:Label width="567"  textAlign="center" paddingTop="10"  fontSize="20"  paddingBottom="5" text="超声申请报告单"/>
			</s:HGroup>
			<s:HGroup width="500">
				<s:Label width="500"  textAlign="left"    fontSize="15"  paddingBottom="5" text="请申请医生认真填写本栏内容："/>
			</s:HGroup>
			<mx:Grid width="100%" height="100%" horizontalGap="1" verticalAlign="middle" verticalGap="10">
				<mx:GridRow>
					<mx:GridItem>
						<s:Label  text="姓        名：" width="100%"  textAlign="left" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label id="F501_03" text="{vf501.f50103}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%"  textAlign="left" text="身份证号码：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label   id="F501_05" text="{vf501.f50105}"/>
					</mx:GridItem>	
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="出生日期：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label   id="F501_06" text="{vf501.f50106Str}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="联系电话：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label   id="F501_07" text="{vf501.f50107}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="体        重：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left"  text="{String(vf501.f50108)}"/><s:Label text="公斤"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="吸烟史：" />
					</mx:GridItem>
					<mx:GridItem width="74">
						<s:CheckBox label="是" mouseEnabled="false" id="f501_09_01"/>
						<s:CheckBox label="否" mouseEnabled="false"  id="f501_09_02"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="糖  尿  病：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox width="42" label="是" mouseEnabled="false" id="f501_10_01"/>
						<s:CheckBox label="否" mouseEnabled="false" id="f501_10_02"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="高血压史：" />
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox label="是" mouseEnabled="false" id="f501_11_01"/>
						<s:CheckBox label="否" mouseEnabled="false" id="f501_11_02"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="IVF：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox width="42" label="是" mouseEnabled="false" id="f501_12_01"/>
						<s:CheckBox label="否" mouseEnabled="false" id="f501_12_02"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="植入日期："/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50113Str}"/>
