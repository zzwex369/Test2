				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="     "/>
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox width="99" label="新生儿死亡史" id="f501_19_05"/>
						<s:CheckBox label="孕期感染史" id="f501_19_07"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox label="开放性神经管" id="f501_19_02"/>
