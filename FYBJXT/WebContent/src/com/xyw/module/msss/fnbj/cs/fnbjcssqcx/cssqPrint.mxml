					<mx:GridItem>
						<s:CheckBox label="单胎" mouseEnabled="false" id="f501_14_01"/>
						<s:CheckBox label="双胎" mouseEnabled="false" id="f501_14_02"/>
						<s:CheckBox label="多胞胎" mouseEnabled="false" id="f501_14_03"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="（胎数："/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label text="{String(vf501.f50115)}"/><s:Label text="）"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" height="16" text="既往超声检查异常：" textAlign="left" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label text="{vf501.f50116}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="末次月经：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50117Str}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="月经周期：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{String(vf501.f50118)}"/><s:Label text="天（不规律者需用B超计算孕周）"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="异常妊娠史：" paddingLeft="20"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="     " />
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox label="21-三体综合症" id="f501_19_01"/>
						<s:CheckBox label="18-三体综合征" id="f501_19_06"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:CheckBox label="死胎史" id="f501_19_04"/>
