						<s:CheckBox label="其他" id="f501_19_08"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="家族史：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50122}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="临床诊断：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50123}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="申请医生：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50124}"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="申请日期："/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50125Str}"/>
					</mx:GridItem>
				</mx:GridRow>
				<mx:GridRow>
					<mx:GridItem>
						<s:Label width="100%" textAlign="left" text="申请单位：" paddingLeft="20"/>
					</mx:GridItem>
					<mx:GridItem>
						<s:Label textAlign="left" text="{vf501.f50126Zh}"/>
					</mx:GridItem>
				</mx:GridRow>
			</mx:Grid>
		</s:VGroup>
		<s:HGroup id="print" horizontalAlign="center" width="568" paddingRight="20" buttonMode="true">
			<s:Button click="printF308()" label="打印" cornerRadius="5" chromeColor="0XFFFFFF"/>
		</s:HGroup>
	</s:VGroup>
</s:TitleWindow>
