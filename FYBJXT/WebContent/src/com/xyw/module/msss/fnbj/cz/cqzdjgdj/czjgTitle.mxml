	</fx:Style>
	<fx:Script>
		<![CDATA[
			import com.xyw.module.msss.fnbj.model.F605;
			import com.xyw.sys.custom.component.CompMethod;
			import com.xyw.sys.model.ComboBoxDataRequest;
			import com.xyw.sys.model.SystemUser;
			
			import flashx.textLayout.operations.CopyOperation;
			
			import mx.collections.ArrayCollection;
			import mx.controls.Alert;
			import mx.controls.DateField;
			import mx.events.FlexEvent;
			import mx.managers.PopUpManager;
			import mx.rpc.events.ResultEvent;
			import mx.utils.StringUtil;
			[Bindable]
			public var vf605:Object;
			public var modify:Boolean=false;
			private var f605:F605;
			private var systemUser:SystemUser ;
			private var comboBoxDataRequest:ComboBoxDataRequest;
			private function closeHandler():void
			{
				var index:Object=this.owner;
				index.refresh();
				PopUpManager.removePopUp(this);
				
			}
			
			private function _initializeComboBox(sql:String,flag:String,showPrompt:Boolean=false):void
			{
				comboBoxDataRequest = new ComboBoxDataRequest();
				comboBoxDataRequest.sql = sql;
				comboBoxDataRequest.flag = flag;
				comboBoxDataRequest.showPrompt = showPrompt;
				this.systemService.getComboBoxData(this.comboBoxDataRequest);
			}
			
			private function creationCompleteHandler(event:FlexEvent):void
			{
				this.title+=!modify?'登记':'修改';
				this.systemUser = this.parentApplication.systemUser;				
				initializeComboBox();
			}

			private function initializeComboBox():void
			{
				
				_initializeComboBox("select t.d101_01, t.d101_02 from D101 t " +
					"where t.d101_01 = '" + this.systemUser.institutionCode +
					"' or t.d101_11 ='" + this.systemUser.institutionCode +"'","D101");
				if(!modify)
				{
					_initializeComboBox("select t.s_01,t.s_02 from S301_10 t where t.S_05='0' " +
						"and t.S_04 ='"+this.systemUser.userCode+"'","S301_10_0");
					
					_initializeComboBox("select t.s_01,t.s_02 from S301_10 t where t.S_05='2' " +
						"and t.S_04 ='"+this.systemUser.userCode+"'","S301_10_2");
					
					_initializeComboBox("select t.s_01,t.s_02 from S301_10 t where t.S_05='7' " +
						"and t.S_04 ='"+this.systemUser.userCode+"'","S301_10_7");
				}
				
			
			}
			
			
			/******************得到数据******************/
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String =comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection =comboBoxDataResponse.comboBoxDataList;
				if(flag =="D101"){
					F605_74.dataProvider = comboBoxDataList;
					F605_74.selectedIndex=CompMethod.getDropDownListSelectedIndex(F605_74.dataProvider,systemUser.institutionCode);
					putVF605();
				}else if(flag =="S301_10_0"){
					F605_72.dataProvider=comboBoxDataList;
					F605_72.selectedIndex=0;
					
				}else if(flag =="S301_10_2"){
					F605_70.dataProvider=comboBoxDataList;
					F605_70.selectedIndex=0;
					
				}else if(flag =="S301_10_7"){
					F605_71.dataProvider=comboBoxDataList;
					F605_71.selectedIndex=0;
					
				}
				
			}
			
			/**
			 *
			 */
			private function putVF605():void
			{
				CompMethod.setRadioButtonSelect(F605_32,vf605.f60532);
				F605_33.text=DateField.dateToString(vf605.f60533,'YYYY-MM-DD');
				F605_08.text=DateField.dateToString(vf605.f60508,'YYYY-MM-DD');
				if(modify)
				{
					F605_44.text=vf605.f60544;
					F605_51.text=vf605.f60551;
					F605_59.text=vf605.f60559;
					F605_61.text=vf605.f60561;
					F605_63.text=vf605.f60563;
					F605_65.text=vf605.f60565;
					F605_67.text=vf605.f60567;
					F605_69.text=vf605.f60569;
					F605_70.text=vf605.f60570;
					F605_71.text=vf605.f60571;
					F605_72.text=vf605.f60572;
					CompMethod.setRadioButtonSelect(F605_46,vf605.f60546);

					CompMethod.setRadioButtonSelect(F605_51,vf605.f60551);
					CompMethod.setRadioButtonSelect(F605_52,vf605.f60552);
					CompMethod.setRadioButtonSelect(F605_53,vf605.f60553);
					CompMethod.setRadioButtonSelect(F605_54,vf605.f60554);
					CompMethod.setRadioButtonSelect(F605_55,vf605.f60555);
					CompMethod.setRadioButtonSelect(F605_56,vf605.f60556);
					CompMethod.setRadioButtonSelect(F605_57,vf605.f60557);
					CompMethod.setRadioButtonSelect(F605_58,vf605.f60558);
					CompMethod.setRadioButtonSelect(F605_60,vf605.f60560);
					CompMethod.setRadioButtonSelect(F605_62,vf605.f60562);
					CompMethod.setRadioButtonSelect(F605_64,vf605.f60564);
					CompMethod.setRadioButtonSelect(F605_66,vf605.f60566);
					CompMethod.setRadioButtonSelect(F605_68,vf605.f60568);
					F605_45.text=DateField.dateToString(vf605.f60545,'YYYY-MM-DD');

					F605_73.text=DateField.dateToString(vf605.f60573,'YYYY-MM-DD');
					F605_74.selectedIndex=CompMethod.getDropDownListSelectedIndex(F605_74.dataProvider,vf605.f60574);
					CompMethod.setCheckBoxAutomationName([F605_50_1,F605_50_2,F605_50_3,F605_50_4,F605_50_9],vf605.f60550);
					CompMethod.setCheckBoxAutomationName([F605_48_1,F605_48_2,F605_48_3,F605_48_4,F605_48_5,F605_48_6,
						F605_48_7,F605_48_8],vf605.f60548)
						
				}
			}
			
			/**
			 *
			 */
			private function validate():void
			{
			
				var checkResult:Array=['',''];
				if(!F605_44.text)
				{
					Alert.show("请输入送检医师","系统提示");
					return;
				}
				if(!F605_45.text)
				{
					Alert.show("请选择标本接受日期","系统提示");
					return;
				}
				
				if(F605_46_2.selected)
				{
					var f60548Arr:Array=CompMethod.getCheckBoxAutomationName([F605_48_1,
										F605_48_2,F605_48_3,F605_48_4,F605_48_5,F605_48_6,
										F605_48_7,F605_48_8]);
					var f60550Arr:Array=CompMethod.getCheckBoxAutomationName([F605_50_1,
										F605_50_2,F605_50_3,F605_50_4,F605_50_9]);
					if(!f60548Arr[0]&&!f60550Arr[0])
					{
						Alert.show("请选择染色体核型分析异常","系统提示");
						return;
					}
					checkResult[0]=f60548Arr[1];
					checkResult[1]=f60550Arr[1];

				}
				if(F605_49.enabled&&!F605_49.text)
				{
					Alert.show("请输入染色体核型数目其他异常","系统提示");
					return;
				}
				
				if(F605_51.enabled&&!F605_51.text)
				{
					Alert.show("请输入染色体核型结构其他异常","系统提示");
					return;
				}
				if(F605_59.enabled&&!F605_59.text)
				{
					Alert.show("请输入FISH异常","系统提示");
					return;
				}
				if(F605_61.enabled&&!F605_61.text)
				{
					Alert.show("请输入染色体微阵列检测异常","系统提示");
					return;
				}
				if(F605_63.enabled&&!F605_63.text)
				{
					Alert.show("请输入CNV-seq异常","系统提示");
					return;
				}
				if(F605_65.enabled&&!F605_65.text)
				{
					Alert.show("请输入全外显子组检测异常","系统提示");
					return;
				}
				if(F605_67.enabled&&!F605_67.text)
				{
					Alert.show("请输入全基因组检测异常","系统提示");
					return;
				}
				if(!F605_69.text)
				{
					Alert.show("请输入电话","系统提示");
					return;
				}
				if(!F605_70.text)
				{
					Alert.show("请输入检测医生","系统提示");
					return;
				}
				if(!F605_71.text)
				{
					Alert.show("请输入审核医生","系统提示");
					return;
				}
				if(!F605_72.text)
				{
					Alert.show("请输入签发医生","系统提示");
					return;
				}
				saveButton.enabled=false;
				saveF605(checkResult);
			}
			
			/**
			 *
			 */
			private function saveF605(checkResult:Array):void
			{
				
				getF605();
				f605.f60548=checkResult[0];
				f605.f60550=checkResult[1];

				f605.f60544=StringUtil.trim(F605_44.text);
				f605.f60551=StringUtil.trim(F605_51.text);
				f605.f60559=StringUtil.trim(F605_59.text);
				f605.f60561=StringUtil.trim(F605_61.text);
				f605.f60563=StringUtil.trim(F605_63.text);
				f605.f60565=StringUtil.trim(F605_65.text);
				f605.f60567=StringUtil.trim(F605_67.text);
				f605.f60569=StringUtil.trim(F605_69.text);
				f605.f60570=StringUtil.trim(F605_70.text);
				f605.f60571=StringUtil.trim(F605_71.text);
				f605.f60572=StringUtil.trim(F605_72.text);
				
				f605.f60546=CompMethod.getRadioButtonValue(F605_46);
				f605.f60552=CompMethod.getRadioButtonValue(F605_52);
				f605.f60553=CompMethod.getRadioButtonValue(F605_53);
				f605.f60554=CompMethod.getRadioButtonValue(F605_54);
				f605.f60555=CompMethod.getRadioButtonValue(F605_55);
				f605.f60556=CompMethod.getRadioButtonValue(F605_56);
				f605.f60557=CompMethod.getRadioButtonValue(F605_57);
				f605.f60558=CompMethod.getRadioButtonValue(F605_58);
				f605.f60560=CompMethod.getRadioButtonValue(F605_60);
				f605.f60562=CompMethod.getRadioButtonValue(F605_62);
				f605.f60564=CompMethod.getRadioButtonValue(F605_64);
				f605.f60566=CompMethod.getRadioButtonValue(F605_66);
				f605.f60568=CompMethod.getRadioButtonValue(F605_68);
				f605.f60545=DateField.stringToDate(F605_45.text,'YYYY-MM-DD');
				f605.f60573=DateField.stringToDate(F605_73.text,'YYYY-MM-DD');
				f605.f60574=F605_74.selectedItem.data;
				f605Service.renewF605(f605);
			}
			
			private function F605_50_09_changeHandler(event:Event,id:TextInput):void
			{
				if(!event.target.selected)
					id.text='';
			}
			
			/**
			 *
			 */
			private function getF605():F605
			{
				f605=new F605();
				var field:String=null;
				var fieldSuffix:String=null;
				for(var i:int=1;i<44;i++)
				{
					fieldSuffix=i<10?'0'+i:i.toString();
					field='f605'+fieldSuffix;
					f605[field]=vf605[field];
				}
				return f605;
			}
			
			
			private function renewF605Handler(event:ResultEvent):void
			{
				var f605Response:Object=event.result;
				if(f605Response.state)
				{
					Alert.show(modify?"修改成功":"保存成功！","系统提示",1,this,
						function():void
						{
							closeHandler();
						});
					return ;
				}
				saveButton.enabled=true;
				Alert.show(f605Response.errorMessage,"系统提示");
			}
		]]>
	</fx:Script>
	
	
	<s:VGroup width="100%" height="100%" chromeColor="#ffffff">
		<s:Scroller width="100%" height="100%" horizontalScrollPolicy="auto"
					verticalScrollPolicy="auto" id="sc">
