						<s:VGroup width="100%" height="100%" gap="1">
						
							<mx:Grid width="100%" height="98" paddingBottom="2" paddingLeft="8">
								<mx:GridRow>
									<mx:GridItem>
										<s:HGroup  gap="15" id="F605_46" verticalAlign="baseline">
											<s:Label text="染色体核型分析"/>	
											<s:RadioButton groupName="F605_46" buttonMode="true" value="1" label="正常" selected="true"
														   change="CompMethod.hasNumEles([F605_46Row_1,F605_46Row_2])"/>
