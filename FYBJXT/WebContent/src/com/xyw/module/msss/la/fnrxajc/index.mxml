			private function finishCase():void
			{
				if(oldId==resL402.l40202)
				{
					Alert.show("不能重复结案","系统提示");
					return;
				}
				//结案前判断
				else if(!(resL402.l40244&&resL402.l40284))
				{
					Alert.show("请先登记乳腺触诊和乳腺彩超检查","系统提示");
					return;
					
				}else if(!resL402.l402165)//初筛临床建议
				{
					Alert.show("请先登记初筛临床建议!","系统提示");
					return;
				}else if(resL402.l40283=='2'||resL402.l40283=='3')//乳腺X线
				{
					if(resL402.l40283=='2'||resL402.l40283=='3')//乳腺X线
					{
						if(resL402.l40283=='2')
						{
							if(!resL402.l402128)
							{
								Alert.show("乳腺彩超建议为乳腺X线检查，请登记乳腺X线检查！","系统提示");
								return;
							}else if(resL402.l402102&&!resL402.l402168)//接受 X线检查 验证是否 登记临床建议
							{
								Alert.show("乳腺X线检查已登记，请先登记乳腺X线临床建议！","系统提示");
								return;
							}
							
							
						}else 
