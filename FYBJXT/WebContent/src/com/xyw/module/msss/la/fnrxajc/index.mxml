			{
				var firstEle:Object=ids is Array ?ids[0]:ids;
				if(firstEle==event.target&&firstEle.selected)
				{
					ids.shift();
					CompMethod.hasNumEles(ids);
				}else
				{
					var select:Boolean=event.target.selected;
					if(firstEle.selected)
						firstEle.selected=!firstEle.selected;
					if(!select)
					{
						var row:Object=event.target.parent.parent;
						CompMethod.hasNumEles(row.getElementAt(row.numElements-1));
					}
					
				}	
				
				
			}
			
		
			
			
