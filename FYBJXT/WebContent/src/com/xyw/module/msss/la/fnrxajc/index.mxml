			/******************得到数据******************/
			private function getComboBoxDataHandler(event:ResultEvent):void
			{
				var comboBoxDataResponse:Object =  event.result;
				var flag:String = comboBoxDataResponse.flag;
				var comboBoxDataList:ArrayCollection = comboBoxDataResponse.comboBoxDataList;
			   if(flag == "LA302_03")
			   {
					L402_23.dataProvider=comboBoxDataList;
					L402_25.dataProvider=comboBoxDataList;
					L402_23.selectedIndex=0;
					L402_25.selectedIndex=0;
				}
				else if(flag == "LA403_06")
				{
					
					L402_52.dataProvider=comboBoxDataList;
					L402_52.selectedIndex=0;
					L402_70.dataProvider=comboBoxDataList;
					L402_70.selectedIndex=0;
				}
			}
			
			
